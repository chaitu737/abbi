<div class="container login" >
	<h2 class="headerabbilogin" >ABBI Login</h2>
	<form ng-controller="loginController">
   <div class="form-group">
    <label for="email">Username:</label>
    <input type="text" class="form-control" id="email" ng-model="login.username"  placeholder="Enter Username">
  </div>
  <div class="form-group">
    <label for="pwd">Password:</label>
    <input type="password" class="form-control" id="pwd" ng-model="login.password" placeholder="Enter password" name="pswd">
  </div>
  <p ng-if="error" ng-style="{color: 'red'}" ng-bind="error"></p>
  <div class="form-group form-check">
    <label class="form-check-label">
      <input class="form-check-input" ng-model="login.remember" type="checkbox" name="remember"> Remember me
    </label>
    <a   type="submit" style="float: right;" href="/forgotpassword">Forgot Password?</a>
  </div>
  <div style="text-align:center;">
   <button class="submitloginbutton" type="button" ng-click="Logins(login);UpdateSessionId(login)" class="btn btn-primary" >Login</button>
 </div>
 <div>
  <p style="text-align: center;"> dont have an account? <a href="/signup">sign up</a></p>
</div>
</form>
