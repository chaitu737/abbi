<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Member Start Date Report</h2>
</div>
<div>
  <div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
    <div class="row"> <br />
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <form>
          <div class="row">
            <div class="col-md-6">     
              <label>Start Date :</label>
              <input class="form-control" ng-model-options="{timezone: 'UTC'}" type="date" ng-model="date.sdate">
            </div>
            <div class="col-md-6">
              <label>End Date :</label>
              <input class="form-control" ng-model-options="{timezone: 'UTC'}" type="date" ng-model="date.edate">
            </div>
          </div>
          <div class="row text-center" style="margin-top:10px;">
            <button class="btn btn-color" type="button" ng-click="MemberStartDateReport(date)">Search</button>
          </div>
        </form>
      </div>
    </div>
    <br>
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-striped table-bordered nowrap" datatable="ng"dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
            <thead>
              <tr>
                <th>Member</th>
                <th>First Name </th>
                <th>Last Name </th>
                <th>Ranch</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>Zip</th>
                <th>Country</th>
                <th>Type</th>
                <th>Join Date</th>
                <th>PaidDate </th>
                <th>DueDate </th>
              </tr>
            </thead>
            <tbody>
              <tr ng-repeat="msd in msds">
                <td><span ng-bind="msd.MemberNumber"></span></td>
                <td><span ng-bind="msd.FirstName"></span></td>
                <td><span ng-bind="msd.LastName"></span></td>
                <td><span ng-bind="msd.RanchName"></span></td>
                <td><span ng-bind="msd.Address"></span></td>
                <td><span ng-bind="msd.City"></span></td>
                <td><span ng-bind="msd.State"></span></td>
                <td><span ng-bind="msd.Zip"></span></td>
                <td><span ng-bind="msd.Country"></span></td>
                <td><span ng-bind="msd.Type"></span></td>
                <td><span ng-bind="msd.JoinDate | date:'MM/dd/yyyy'"></span></td>
                <td><span ng-bind="msd.PaidDate | date:'MM/dd/yyyy'"></span></td>
                <td><span ng-bind="msd.DueDate | date:'MM/dd/yyyy'"></span></td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>