<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Registration Prices</h2>
</div>
<div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" ng-init="viewRegPrices()">
  <div class="row"> <br />
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
      <button class="btn btn-color" type="button" data-toggle="modal" data-target="#NewRegistrationPrice" ng-click="getdata()">New Registration Price</button>
    </div>
    <div class="modal fade" id="NewRegistrationPrice" role="dialog">
      <div class="modal-dialog modal-lg">
        <div class="modal-content">
          <div class="modal-header">
            <button class="close" type="button" data-dismiss="modal">Ã—</button>
            <h4 class="modal-title text-center">New Membership</h4>
          </div>
          <div class="modal-body">
            <div class="container edge" style="background-color: #d8ceb9;height:100%;width:100%;padding: 10px;">
              <form class="form-group">
                <div class="row">
                  <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                    <div class="form-group">
                      <label for="Category">Birth Year :</label>
                      <input type="text" ng-model="nrp.BirthYear" class="form-control">
                    </div>
                  </div>
                  <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12 ">
                    <div class="form-group">
                      <label for="events">Gender:</label>
                      <select class="form-control" ng-model="nrp.Gender">
                        <option value="">Both</option>
                        <option value="M">Bull</option>
                        <option value="F">Cow</option>
                      </select>
                    </div>
                  </div>
                  <div>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                    <div class="form-group">
                      <label for="Category">Reg 0-1 yr $ :</label>
                      <input type="text" ng-model="nrp.Reg_1yr" class="form-control">
                    </div>
                  </div>
                  <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12 ">
                    <div class="form-group">
                      <label for="events">Reg 1-2 yr $:</label>
                      <input type="text" ng-model="nrp.Reg_2yr" class="form-control">
                    </div>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                    <div class="form-group">
                      <label for="Category">Reg 2+ yr $ :</label>
                      <input type="text" ng-model="nrp.Reg_2yr_older" class="form-control">
                    </div>
                  </div>
                  <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12 ">
                    <div class="form-group">
                      <label for="events">Cat Only $:</label>
                      <input type="text" ng-model="nrp.CatOnly" class="form-control">
                    </div>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                    <div class="form-group">
                      <label for="Category">Cat Upgrade $ :</label>
                      <input type="text" ng-model="nrp.CatUpgrade" class="form-control">
                    </div>
                  </div>
                  <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12 ">
                    <div class="form-group">
                      <label for="events">New Memberv 0-1 yr $:</label>
                      <input type="text" ng-model="nrp.NewMember" class="form-control">
                    </div>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                    <div class="form-group">
                      <label for="Category">New Member 1+ yr $ :</label>
                      <input type="text" ng-model="nrp.NewMember_1yr_older" class="form-control">
                    </div>
                  </div>
                  <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12 ">
                    <div class="form-group">
                      <label for="events">Rush $:</label>
                      <input type="text" ng-model="nrp.RushRegistration" class="form-control">
                    </div>
                  </div>
                </div>
              </form>
              <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12 form-group">
                <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12 form-group"
                style="white-space: nowrap;">
                <button class="btn btn-color" type="button" ng-click="AddRegPrice(nrp)">Save</button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="panel panel-default">
  <div class="panel-body">
    <div class="table-responsive">
      <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
        <thead>
          <tr>
            <th>Birth Year</th>
            <th>Gender </th>
            <th>Reg 0-1 yr ($)</th>
            <th>Reg 1-2 yr ($)</th>
            <th>Reg 2+ yr ($)</th>
            <th>Cat only  ($)</th>
            <th>Cat Upgrade ($)</th>
            <th>New Member 0-1 yr ($)</th>
            <th>New Member 1+ yr ($)</th>
            <th>Rush ($)</th>
            <th>Edit </th>
          </tr>
        </thead>
        <tbody>
          <tr ng-repeat="vrp in vrps">
            <td>{{vrp.BirthYear}}</td>
            <td>{{vrp.Gender}}</td>
            <td>{{vrp.Reg_1yr | currency}}</td>
            <td>{{vrp.Reg_2yr | currency}}</td>
            <td>{{vrp.Reg_2yr_older | currency}}</td>
            <td>{{vrp.CatOnly | currency}}</td>
            <td>{{vrp.CatUpgrade | currency}}</td>
            <td>{{vrp.NewMember | currency}}</td>
            <td>{{vrp.NewMember_1yr_older | currency}}</td>
            <td>{{vrp.RushRegistration | currency}}</td>
            <td>
              <i title="Edit" ng-click="editRegPrice(vrp)" class="btn btn-warning fa fa-edit"></i>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
