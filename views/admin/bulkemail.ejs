<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Bulk Email</h2>
</div>
<div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
  <div class="row"> <br />
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" ng-init="ViewBulkEmail()">
      <button class="btn btn-color" type="button" ng-click="DeleteBulkEmail(bes)">Clear Updated Fields Table</button>
    </div>
  </div>
  <div class="panel panel-default">
    <div class="panel-body">
      <div class="table-responsive">
        <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
          <thead>
            <tr>
              <th>Member</th>
              <th>Field Changed </th>
              <th>Old Value </th>
              <th>New Value </th>
              <th>Date Changed</th>
            </tr>
          </thead>
          <tbody>
            <tr ng-repeat="be in bes">
              <td>{{be.membernumber}}</td>
              <td>{{be.field}}</td>
              <td>{{be.oldvalue}}</td>
              <td>{{be.newvalue}}</td>
              <td>{{be.datestamp | date:'MM/dd/yyyy'}}</td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>    
</div>