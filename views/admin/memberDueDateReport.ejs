<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Member Due Date Report</h2>
</div>
<div>
  <div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
    <div class="row"> <br />
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <form>
          <div class="row">
            <div class="col-md-6">     
              <label>Start Date :</label>
              <input class="form-control" type="date" ng-model-options="{timezone: 'UTC'}" ng-model="dates.sdate">
            </div>
            <div class="col-md-6">
              <label>End Date :</label>
              <input class="form-control" type="date" ng-model-options="{timezone: 'UTC'}" ng-model="dates.edate">
            </div>
          </div>
          <div class="row text-center" style="margin-top:10px;">
            <button class="btn btn-color" type="button" ng-click="MemberDueDateReport(dates)">Search</button>
          </div>
        </form>
      </div>
    </div>
    <br>
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
            <thead>
              <tr>
                <th>Member</th>
                <th>PaidDate </th>
                <th>DueDate </th>
                <th>First Name </th>
                <th>Last Name </th>
                <th>Ranch</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>Zip</th>
                <th>Country</th>
                <th>Type</th>
              </tr>
            </thead>
            <tbody>
              <tr ng-repeat="mdd in mdds">
                <td><span ng-bind="mdd.MemberNumber"></span></td>
                <td><span ng-bind="mdd.PaidDate | date:'MM/dd/yyyy'"></span></td>
                <td><span ng-bind="mdd.DueDate  | date:'MM/dd/yyyy'"></span></td>
                <td><span ng-bind="mdd.FirstName"></span></td>
                <td><span ng-bind="mdd.LastName"></span></td>
                <td><span ng-bind="mdd.RanchName"></span></td>
                <td><span ng-bind="mdd.Address"></span></td>
                <td><span ng-bind="mdd.City"></span></td>
                <td><span ng-bind="mdd.State"></span></td>
                <td><span ng-bind="mdd.Zip"></span></td>
                <td><span ng-bind="mdd.Country"></span></td>
                <td><span ng-bind="mdd.Type"></span></td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>