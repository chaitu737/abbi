<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Member Payments</h2>
</div>
<div class="container" style="background-color: #d8ceb9;height:100%;width:80%;padding: 10px;" ng-controller="MemberPaymentController">
  <div ng-show="isShow">
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
        <label>Pay Status </label>
        <select class="form-control" ng-model="memberpay.status">
          <option selected="selected" value="">Any</option>
          <option value="Open">Open</option>
          <option value="Closed">Closed</option>
          <option value="Declined">Declined</option>
          <option value="Processing">Processing</option>
        </select>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
        <label>Category</label>
        <select class="form-control" ng-model="memberpay.category">
          <option selected="selected" value="">All</option>
          <option value="Work Order">Work Order</option>
          <option value="Animal Registration">Animal Registration</option>
          <option value="Event Registration">Event Registration</option>
          <option value="Online Store">Online Store</option>
          <option value="New Membership">New Membership</option>
          <option value="Membership Renewal">Membership Renewal</option>
          <option value="Membership Upgrade">Membership Upgrade</option>
          <option value="USBBA Member">USBBA Member</option>
          <option value="USBBA General">USBBA General</option>
          <option value="American Heritage">American Heritage</option>
          <option value="Custom Payment">Custom Payment</option>
        </select>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" ng-init="updatemember()">
        <label>Member :</label>
        <input class="form-control" type="text" ng-model="memberpay.member" list="names2">
            <datalist id="names2" ng-model="MemberNumbersss">
              <option ng-repeat="member in members | filter:memberpay.member | limitTo:8" value="{{member.MemberNumber}} - {{member.MemberName}}"></option>
            </datalist>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
        <label>Search</label>
        <input type="text" class="form-control" ng-model="memberpay.search" value="">
      </div>
    </div>
    <br>
    <div>
      <button class="btn btn-color" id="loaderbtn" ng-click="ViewMemberPayments(memberpay)">View Payments</button>
      <button class="btn btn-color" ng-click="ResetPayments(memberpay)">Reset</button>
    </div>
    <hr>
    <br>
    <div class="row">
      <div class="col-md-6 col-lg-6 col-sm-6 col-xs-12">
        <select class="form-control" ng-model="AddNewPayment.name">
          <option value="">--Select One--</option>
          <option ng-option value="WorkOrderMember">Work Order</option>
          <option ng-option value="AnimalRegistrationMember">Animal Registration</option>
          <!-- <option ng-option value="EventRegistrationMember">Event Registration</option> -->
          <option ng-option value="OnlineStoreMember">Online Store</option>
          <option ng-option value="NewMembershipMember">New Membership</option>
          <option ng-option value="MembershipRenewalMember">Membership Renewal</option>
          <option ng-option value="MembershipUpgradeMember">Membership Upgrade</option>
          <!-- <option ng-option value="usbbaMember">USBBA Member</option> -->
        </select>
      </div>
      <div> 
        <button class="btn btn-color" type="button" ng-click="changeme();DeleteProducts()">Add New</button>
      </div>
    </div>
  </div>
  <div ng-if="AddNewPayment.name == 'WorkOrderMember'" ng-show="isHide">
    <div class="row">
      <div class="col-md-6 col-lg-6 col-sm-6 col-xs-12">
        <label> Work Order Number: </label>
        <input type="text" ng-model="woor.number">
    <button class="btn btn-color" ng-click="GetWorkOrder(woor)" ng-mousedown="calculateAmount($scope.woords,$scope.PaymentType)">Get Work Data</button>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
        <button type="button" class="btn btn-color" ng-click="save()">Exit</button>
      </div>
    </div>
  </div>
  <div ng-show="isHide1" id="printscreen">
    <div class="row">
      <div class="col-md-6 col-lg-6 col-sm-6 col-xs-12">
        <label>Category : </label>
        <span style="color:red; font-size:21px;">Work Order</span>
      </div>
      <div class="col-md-6 col-lg-6 col-sm-6 col-xs-12">
        <label>Date : </label>
        <span style="color:black" ng-bind="woords.PaymentDate | date : MM/dd/yyyy"></span>
      </div>
    </div>
    <div class="row">
      <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
        <label>Member Name : </label>
        <span style="color:black">{{woords.FirstName+" "+woords.LastName}}</span>
        <br>
        <span style="color:black" ng-bind="woords.BillingAddress"></span>
        <br>
        <span style="color:black" ng-bind="woords.BillingCity"></span>
        <br>
        <span style="color:black" ng-bind="woords.BillingState"></span>,
        <span style="color:black" ng-bind="woords.BillingZip"></span>,
        <span style="color:black" ng-bind="woords.BillingCountry"></span>
        <br>
        <span style="color:black" ng-bind="woords.Phone"></span>
      </div>
    </div>
    <br>
    <hr>
    <div class="row">
      <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
        <label>Ship To : </label>
        <span style="color:black" ng-bind="woords.ShippingFirstName"></span>
        <br>
        <span style="color:black" ng-bind="woords.ShippingAddress"></span>
        <br>
        <span style="color:black" ng-bind="woords.ShippingCity"></span>
        <br>
        <span style="color:black" ng-bind="woords.ShippingState"></span>,
        <span style="color:black" ng-bind="woords.ShippingCountry"></span>,
        <span style="color:black" ng-bind="woords.ShippingZip"></span>
      </div>
    </div>
    <br>
    <div>
      <label>Work Order :</label>
      <a ng-click="EditWorkOrder(woords);ShowWorkOrder()"><span ng-bind="woords.WorkOrderNumber"></span></a>
      <br>
      <label>Work Order Date :</label>
      <span style="color:black" ng-bind="woords.WorkOrderDate | date : 'MM/dd/yyyy'"></span>
    </div>
    <br>
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-striped table-bordered nowrap" ng-repeat="head in header | unique : 'Category'">
            <thead>
              <tr>
                <th width="500px"><span ng-bind="head.Category"></span><span ng-if="head.note" ng-bind="head.note"></span></th>
                <th>Quantity</th>
                <th>Price ($)</th>
                <th>Postage</th>
                <th>Subtotal ($)</th>
              </tr>
            </thead>
            <tbody>
              <tr ng-if="head.categoryid==wotable1.Categoryid" ng-repeat="wotable1 in wotable">
                <td><span ng-bind="wotable1.Description"></span></td>
                <td><span ng-bind="wotable1.Quantity"></span></td>
                <td><span ng-bind="wotable1.Price | currency"></span></td>
                <td><span ng-bind="wotable1.postage"></span></td>
                <td><span ng-bind="wotable1.SubTotal | currency"></span></td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <br>
    <h2>Payment Information</h2>
    <div class="row">
      <div class="col-md-12 col-lg-12 col-sm-6 col-xs-6">
        <label> Amount ($)</label>
        <table>
        <thead>
          <tr>
            <th>Sub Total</th>
            <th>Handling</th>
            <th>Discount</th>
            <th>Total Amount</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>
              <input type="text" placeholder="Subtotal" ng-model="woords.Total" ng-change="CalculateAmount(woords,PaymentType)">+
            </td>
            <td>
              <input type="text" placeholder="Handling" ng-model="woords.HandlingFeeAmt" ng-change="CalculateAmount(woords,PaymentType)">+
            </td>
            <td>
              <input type="text" placeholder="Discount" ng-model="woords.HandlingDiscountAmt" ng-change="CalculateAmount(woords,PaymentType)">=
            </td>
            <td>
              <input type="text" ng-model="woords.Tot">
            </td>
          </tr>
        </tbody>
      </table>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
        <label>Payment Type</label>
        <select ng-model="PaymentType.Payment" ng-model="PaymentType.Payment" class="form-control" ng-change="CalculateAmount(woords,PaymentType)">
          <option ng-option selected="selected" value="-1">-Select One-</option>
          <option ng-option value="VI">Visa</option>
          <option ng-option value="MC">Master Card</option>
          <option ng-option value="AMEX">American Express</option>
          <option ng-option value="Discover">Discover</option>
          <option ng-option value="Auth.Net">Authorize.Net</option>
          <option ng-option value="Check">Check</option>
          <option ng-option value="eCheck">e-Check</option>
          <option ng-option value="Money Order">Money Order</option>
          <option ng-option value="Cashier Check">Cashier Check</option>
        </select>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-2 col-lg-2 col-sm-2 col-xs-2"></div>
      <div ng-if="PaymentType.Payment == 'undefined' ||PaymentType.Payment == '-1' || PaymentType.Payment == 'VI' || PaymentType.Payment == 'MC' || 
      PaymentType.Payment == 'AMEX' || PaymentType.Payment == 'Discover' || PaymentType.Payment == 'Check' ||  
      PaymentType.Payment == 'Money Order' || PaymentType.Payment == 'Cashier Check'">
      <div class="col-md-10 col-lg-10 col-sm-10 col-xs-10">

        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Credit Card No</label>
          <input type="number" placeholder="Credit Card No." class="form-control" ng-model="PaymentType.cc">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Exp. (MM/YY)</label>
          <input type="text" placeholder="Exp. (MM/YY)" class="form-control" ng-model="PaymentType.expire">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Sec Code</label>
          <input type="number" placeholder="Sec Code" class="form-control" ng-model="PaymentType.cvv">
        </div>
      </div>
    </div>

    <div ng-if="PaymentType.Payment == 'Auth.Net'">
      <div class="col-md-10 col-lg-10 col-sm-10 col-xs-10">

        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Credit Card No</label>
          <input type="number" placeholder="Credit Card No." class="form-control" ng-model="PaymentType.cc">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Exp. (MM/YY)</label>
          <input type="text" placeholder="Exp. (MM/YY)" class="form-control" ng-model="PaymentType.expire">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Sec Code</label>
          <input type="number" placeholder="Sec Code" class="form-control" ng-model="PaymentType.cvv">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Auth No.</label>
          <input type="number" placeholder="Auth No." class="form-control" ng-model="PaymentType.authno">
        </div>
      </div>
    </div>

    <div ng-if="PaymentType.Payment == 'eCheck'">
      <div class="col-md-10 col-lg-10 col-sm-10 col-xs-10">

        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label> Account No</label>
          <input type="number" placeholder="Account No." class="form-control" ng-model="PaymentType.acno">
        </div>
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>Account Holder's Name</label>
          <input type="text" placeholder="Account Holder's Name.." class="form-control" ng-model="PaymentType.acname">
        </div>
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>Account Type</label>
          <select class="form-control" ng-init="PaymentType.actype='checking'" ng-model="PaymentType.actype">
            <option value="checking"> Checking</option>
            <option value="savings"> Savings</option>
          </select>
        </div>
      </div>
      <br>
      <div class="col-md-10 col-lg-10 col-sm-10 col-xs-10" style="float: right;">

        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>ABA Routing No.</label>
          <input type="number" placeholder="ABA Routing No." class="form-control" ng-model="PaymentType.aba">
        </div>
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>Bank Name</label>
          <input type="text" placeholder="Bank Name.." class="form-control" ng-model="PaymentType.bank">
        </div>

      </div>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
      <label>Status</label>
      <select ng-model="woords.Status" ng-init="woords.Status='Open'" class="form-control">
        <option ng-option value="Open">Open</option>
        <option ng-option value="Close">Close</option>
        <option ng-option value="Declined">Declined</option>
        <option ng-option value="Processing">Processing</option>
      </select>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
      <label> Order Description <span style="color:black">(in addition to line items)</span> </label>
      <textarea ng-model="woords.description" class="form-control"></textarea>
    </div>
    <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
      <label>Admin Notes</label>
      <textarea ng-model="woords.Notes" class="form-control"></textarea>
    </div>
  </div>
  <br>
  <div class="container border" style="background-color: #d8ceb9;height:100%;width:100%;padding: 10px;">
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-striped table-bordered nowrap" datatable="ng"
          dt-options="vm.dtOptions" dt-instance="vm.dtInstance">
          <thead>
            <tr>
              <th>RegNo</th>
              <th>Animal Name</th>
              <th>Herd #</th>
              <th>Sex</th>
              <th>Horns</th>
              <th>Birthdate</th>
            </tr>
          </thead>
          <tbody>
            <tr ng-repeat="anim in animal">
              <td><span ng-bind="anim.RegNo"></span></td>
              <td><span ng-bind="anim.Animal"></span></td>
              <td><span ng-bind="anim.PrivateHerd"></span></td>
              <td><span ng-bind="anim.typeservice"></span></td>
              <td><span ng-bind="anim.Horns"></span></td>
              <td><span ng-bind="anim.Birthdate | date : 'MM/dd/yyyy'"></span></td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
<br>
<div>
  <button class="btn btn-color" ng-click="AnimalTableData(woords)">View Existing Animals</button>
</div>
<br>
<div class="container border" style="background-color: #d8ceb9;height:100%;width:100%;padding: 10px;">
  <div class="panel panel-default">
    <div class="panel-body">
      <div class="table-responsive">
        <table class="table table-striped table-bordered nowrap" datatable="ng"
        dt-options="vm.dtOptions" dt-instance="vm.dtInstance">
        <thead>
          <tr>
            <th>RegNo</th>
            <th>Animal Name</th>
            <th>Herd #</th>
            <th>Birthdate</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <tr ng-repeat="gatd in gatds">
            <td><span ng-bind="gatd.Regno"></span></td>
            <td><span ng-bind="gatd.Animal"></span></td>
            <td><span ng-bind="gatd.PrivateHerd"></span></td>
            <td><span ng-bind="gatd.birthdate | date : 'MM/dd/yyyy'"></span></td>
            <td>
              <span ng-if="gatd.EIDOrder!=''" ng-bind="gatd.EIDOrder"></span>
              <input ng-if="gatd.EIDOrder==null" type="checkbox" ng-click="CheckExistingAnimal(gatd,woords)">
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
</div>
<br>
<div class="row">
  <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
    <button type="button" class="btn btn-color" ng-click="SaveWorkOrderPay(woords,PaymentType)">Save</button>
    <button type="button" class="btn btn-color" ng-click="save()">Exit</button>
  </div>
</div>
</div>
<div class="container" style="padding: 10px;" ng-show="editworkorder" print-remove>
  <form>
    <div class="container edge" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
      <h3 class="text-center text-primary">Work Order # : {{ework.WorkOrderNumber}}</h3>
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="row">
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <div class="form-group">
              <label>Member</label>
              <div ng-hide="showwork">
                <input ng-init="updatemember()" class="form-control" type="text" ng-model="ework.member" list="name">
                <datalist id="name" ng-model="member">
                  <option ng-repeat="member in members | filter:ework.member | limitTo:8" value="{{member.MemberNumber}} - {{member.MemberName}}"></option>
                </datalist>
              </div>
              <div ng-show="showwork">
                <input ng-init="updatemember()" class="form-control" type="text" ng-model="ework.MemberNumber" list="names" ng-change="EditMemberWorkorder(ework)">
                <datalist id="names" ng-model="MemberNumbers">
                  <option ng-repeat="member in members | filter:ework.member | limitTo:8" value="{{member.MemberNumber}} - {{member.MemberName}}"></option>
                </datalist>
              </div>
            </div>
          </div>
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <div class="form-group">
              <label>WO Date</label>
              <input type="text" ng-model="ework.CreatedDate" class="form-control">
            </div>
          </div>
        </div>
      </div>
    </div>
    <br>
    <div class="container edge" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
      <h3 class="text-center text-primary">Billing Information</h3>
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>First Name</label>
            <input type="text" class="form-control" ng-model="ework.FirstName">
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>Last Name</label>
            <input type="text" class="form-control" ng-model="ework.LastName">
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>Email</label>
            <input type="text" class="form-control" ng-model="ework.Email">
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>Phone</label>
            <input type="text" class="form-control" ng-model="ework.Phone">
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>Membership Type</label>
            <span class="bg-danger" ng-bind="ework.Type"></span>
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>Membership Due</label>
            <input type="text" class="form-control" ng-model="ework.DueDate">
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
          <div class="form-group">
            <label>Address</label>
            <textarea class="form-control" ng-model="ework.BillingAddress"></textarea>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>City</label>
            <input type="text" class="form-control" ng-model="ework.BillingCity">
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>State</label>
            <input type="text" class="form-control" ng-model="ework.BillingState">
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>Zip Code</label>
            <input type="text" class="form-control" ng-model="ework.BillingZip">
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>Country</label>
            <input type="text" class="form-control" ng-model="ework.BillingCountry">
          </div>
        </div>
      </div>
    </div>
    <br>
    <div class="container edge table-responsive" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="table-responsive">
            <table class="table table-bordered table-striped report" ng-repeat="head in heading | unique : 'Header'">
              <thead>
                <tr>
                  <th width="100px">Complete</th>
                  <th width="500px"><span>{{head.Header}}</span></th>
                  <th width="100px">Quantity</th>
                  <th width="100px">Price ($)</th>
                  <th width="100px">Postage</th>
                  <th width="100px">Type</th>
                  <th width="100px">Subtotal ($)</th>
                </tr>
              </thead>
              <tbody>
                <tr ng-if="head.Categoryid==eworks.Categoryid" ng-repeat="eworks in ework1">
                  <td width="100px" ng-if="eworks.Quantity==null">
                  </td>
                  <td width="100px" ng-if="eworks.Quantity > 0">
                    <input type="checkbox" ng-model="eworks.LineItemComplete">
                  </td>
                  <td width="500px">
                    <span ng-bind="eworks.Description"></span>
                  </td>
                  <td ng-if="eworks.NewBreederOnly==true"><input class="form-control" type="number" ng-change="ChangeValue(eworks,ework)" ng-model="eworks.Quantity" disabled=""></td>
                  <td ng-if="eworks.NewBreederOnly==false"><input class="form-control" type="number" ng-change="ChangeValue(eworks,ework)" ng-model="eworks.Quantity"></td>
                  <td ng-if="eworks.NewBreederOnly==true"><input class="form-control" type="number" ng-change="ChangeValue(eworks,ework)" ng-model="eworks.Price" disabled=""></td>
