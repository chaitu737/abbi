  <div class="panel-body">
    <div class="table-responsive">
      <table id="example" class="table table-striped table-bordered nowrap" datatable="ng"
      dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
      <thead>
        <tr>
          <th>Close all</th>
          <th></th>
          <th>Pmt#</th>
          <th>Parent Pmt</th>
          <th>Member#</th>
          <th>Name</th>
          <th>Date</th>
          <th>Category</th>
          <th>Event</th>
          <th>Status</th>
        </tr>
      </thead>
      <tbody>
        <tr ng-repeat="vmp in vmps | pagination: curPage * pageSize | limitTo: pageSize">
          <td>
            <input type="checkbox" ng-click="CheckClosedPayment(vmp)">
          </td>
          <td ng-if="vmp.FromTheWeb==1">
            <img src="../images/active.gif" alt="active">
          </td>
          <td ng-if="vmp.FromTheWeb!=1"></td>
          <td>
            <a style="color: blue; text-decoration: underline;cursor: pointer;" ng-click="EditMemberPayment(vmp,1);storePay(vmp);displayTable(vmp.PaymentID)"><span style="color:black" ng-bind="vmp.PaymentID"></span></a>
          </td>
          <td>
            <a style="color: blue; text-decoration: underline;cursor: pointer;" ng-click="EditMemberPayment(vmp,2);storePay(vmp);displayTable(vmp.ParentPaymentId)"><span style="color:black" ng-bind="vmp.ParentPaymentId"></span></a> 
          </td>
          <td><span style="color:black" ng-bind="vmp.MemberNumber"></span></td>
          <td><span style="color:black" ng-bind="vmp.MemberName"></span></td>
          <td><span style="color:black" ng-bind="vmp.PaymentDate | date : 'MM/dd/yyyy'"></span></td>
          <td><span style="color:black" ng-bind="vmp.Category"></span></td>
          <td><span style="color:black" ng-bind="vmp.EventName"></span></td>
          <td><span style="color:black" ng-bind="vmp.Status"></span></td>
        </tr>
      </tbody>
    </table>
    <div class="dataTables_wrapper no-footer" ng-if="!selecteddropdown">
      <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
        </div>
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
    <div class="pagination pagination-centered" ng-show="membercount" style="margin: 0;">
      <ul class="pagination-controle pagination">
       <li>
        <button type="button" class="btn btn-primary" ng-disabled="curPage == 0"
       ng-click="curPage=curPage-1;Viewmempay();" style="float: left;"> &lt; PREV</button>
       </li>
       <li>
       <span>Page {{curPage + 1}} of {{ numberOfPages() }}</span>
       </li>
       <li>
       <button type="button" class="btn btn-primary"
       ng-disabled="curPage >= membercount/pageSize - 1"
       ng-click="curPage = curPage+1;Viewmempay();" >NEXT &gt;</button>
       </li>
      </ul>
      </div>
      </div>
      </div>
      <div class="dataTables_wrapper no-footer" ng-if="selecteddropdown">
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
          </div>
          <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
      <div class="pagination pagination-centered" ng-show="membercount" style="margin: 0;">
        <ul class="pagination-controle pagination">
         <li>
          <button type="button" class="btn btn-primary" ng-disabled="curPage == 1"
         ng-click="curPage=curPage-1;ViewMemberPayments();" style="float: left;"> &lt; PREV</button>
         </li>
         <li>
         <span>Page {{curPage}} of {{ numberOfPages() }}</span>
         </li>
         <li>
         <button type="button" class="btn btn-primary"
         ng-disabled="curPage >= membercount/pageSize - 1"
         ng-click="curPage = curPage+1;ViewMemberPayments();" >NEXT &gt;</button>
         </li>
        </ul>
        </div>
        </div>
        </div>
  </div>
</div>
</div>
