      <input class="form-control" type="text" ng-model="evenreg.parent2">
      <br>
      <div>
        <button class="btn btn-color">Submit</button>
        <button class="btn btn-color" ng-click="ExitAddAni()">Exit</button>
      </div>
    </div>
  </div>
</div>
<div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" ng-hide="additional">
  <div class="row">
    <div class="col-md-6 col-lg-6 col-sm-6 col-xs-12">
      <label>Category : </label>
      <span style="color:red; font-size:21px;" ng-bind="emps.Category"></span>
    </div>
    <div class="col-md-6 col-lg-6 col-sm-6 col-xs-12">
      <label>Date : </label>
      <span style="color:black" ng-bind="emps.PaymentDate | date : MM/dd/yyyy"></span>
    </div>
  </div>
  <br>
  <div ng-if="emps.Category=='Work Order'">
    <label>Work Order :</label>
    <span style="color:black" ng-bind="wods.WorkOrderNumber"></span>
    <br>
  </div>
  <div>
    <h2 style="text-align: center;color: #2c3e50;font-variant: all-petite-caps;">Billing Information</h2>
    <div class="col-md-12">
      <div class="col-md-6">
        <label>Member Name :</label>
        <input type="text" class="form-control" ng-model="personData.memberName">
        <label>Member Id :</label>
        <input type="text" class="form-control" ng-model="personData.MemberNumber">
        <label>Membership Type :</label>
        <input type="text" class="form-control" ng-model="personData.Type">
        <label>Membership Due :</label>
        <span class="form-control" ng-bind="gais.PaymentDate | date : 'MM/dd/yyyy'"></span>
        <label>Email :</label>
        <input type="text" class="form-control" ng-model="personData.Email">
        <label>Phone :</label>
        <input type="text" class="form-control" ng-model="personData.Phone">
      </div>
      <div class="col-md-6">
        <label>Address :</label>
        <input type="text" class="form-control" ng-model="personData.Address">
        <label>City :</label>
        <input type="text" class="form-control" ng-model="personData.City">
        <label>State :</label>
        <input type="text" class="form-control" ng-model="personData.State">
        <label>Zip Code :</label>
        <input type="text" class="form-control" ng-model="personData.Zip">
        <label>Country :</label>
        <input type="text" class="form-control" ng-model="personData.Country">
      </div>
    </div>
  </div>
  <br>
  <br>
  <h2 style="text-align: center;color: #2c3e50;font-variant: all-petite-caps;">Payment Information</h2>
  <table style="border: 1px solid;border-collapse: collapse;">
    <tr>
      <th style="border: 1px solid;">Payment</th>
      <th style="border: 1px solid;">Type</th>
      <th style="border: 1px solid;">Card</th>
      <th style="border: 1px solid;">Amount</th>
    </tr>
    <tr ng-repeat="obj in tableData">
      <td style="border: 1px solid;" ng-if="obj.paymentclass == 'Parent Payment'">{{obj.PaymentID}} - ({{obj.paymentclass}})</td>
      <td style="border: 1px solid;text-decoration: underline;cursor: pointer;" ng-if="obj.paymentclass == 'Additional Payment'"  ng-click="openSection(obj.PaymentID,1)">{{obj.PaymentID}} - ({{obj.paymentclass}})</td>
      
      <td style="border: 1px solid;">{{obj.PaymentType ? obj.PaymentType : 'Other'}}</td>
      <td style="border: 1px solid;">{{obj.PaymentType == 'eCheck' ? obj.eChkAcctNo : obj.CCNumber}}</td>
      <td style="border: 1px solid;">${{obj.PaymentAmount}}</td>
    </tr>
  </table>
  <br>
  <h2 style="text-align: center;color: #2c3e50;font-variant: all-petite-caps;">Add Additional Payment</h2>
  <div class="row">
    <div class="col-md-12 col-lg-12 col-sm-6 col-xs-6">
      <label> Amount ($)</label>
      <table>
        <thead>
          <tr>
            <th>Sub Total</th>
            <th>Handling</th>
            <th>Discount</th>
            <th>Total Amount</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>
              <input type="number" placeholder="Subtotal" ng-model="addPaymentInfo.SubtotalAmt" ng-change="CalculateCount3(addPaymentInfo,PaymentType)">+
            </td>
            <td>
              <input type="number" placeholder="Handling" ng-model="addPaymentInfo.HandlingFeeAmt" ng-change="CalculateCount3(addPaymentInfo,PaymentType)">+
            </td>
            <td>
              <input type="number" placeholder="Discount" ng-model="addPaymentInfo.DiscountAmt" ng-change="CalculateCount3(addPaymentInfo,PaymentType)">=
            </td>
            <td>
              <input type="number" ng-model="addPaymentInfo.PaymentAmount">
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
      <label>Payment Type</label>
      <select ng-change="CalculateCount3(addPaymentInfo,PaymentType)" ng-model="PaymentType.Payment" class="form-control">
        <option ng-option selected="selected" value="">-Select One-</option>
        <option ng-option value="VI">Visa</option>
        <option ng-option value="MC">Master Card</option>
        <option ng-option value="AMEX">American Express</option>
        <option ng-option value="Discover">Discover</option>
        <option ng-option value="Auth.Net">Authorize.Net</option>
        <!-- <option ng-option value="Check">Check</option> -->
        <option ng-option value="eCheck">e-Check</option>
        <option ng-option value="other">Other</option>
        <!-- <option ng-option value="Money Order">Money Order</option> -->
        <!-- <option ng-option value="Cashier Check">Cashier Check</option> -->
      </select>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="col-md-2 col-lg-2 col-sm-2 col-xs-2"></div>
    <div ng-if="PaymentType.Payment == 'undefined' ||PaymentType.Payment == '-1' || PaymentType.Payment == 'VI' || PaymentType.Payment == 'MC' || PaymentType.Payment == 'AMEX' || PaymentType.Payment == 'Discover' || PaymentType.Payment == 'Check' ||  PaymentType.Payment == 'Money Order' || PaymentType.Payment == 'Cashier Check'">
      <div class="col-md-10 col-lg-10 col-sm-10 col-xs-10">
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Credit Card No</label>
          <input type="number" placeholder="Credit Card No." class="form-control" ng-model="PaymentType.cc">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Exp. (MM/YY)</label>
          <input type="text" placeholder="Exp. (MM/YY)" class="form-control" ng-model="PaymentType.expire">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Sec Code</label>
          <input type="number" placeholder="Sec Code" class="form-control" ng-model="PaymentType.cvv">
        </div>
      </div>
    </div>
    <div ng-if="PaymentType.Payment == 'Auth.Net'">
      <div class="col-md-10 col-lg-10 col-sm-10 col-xs-10">
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Credit Card No</label>
          <input type="number" placeholder="Credit Card No." class="form-control" ng-model="PaymentType.cc">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Exp. (MM/YY)</label>
          <input type="text" placeholder="Exp. (MM/YY)" class="form-control" ng-model="PaymentType.expire">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Sec Code</label>
          <input type="number" placeholder="Sec Code" class="form-control" ng-model="PaymentType.cvv">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Auth No.</label>
          <input type="number" placeholder="Auth No." class="form-control" ng-model="PaymentType.acno">
        </div>
      </div>
    </div>
    <div ng-if="PaymentType.Payment == 'eCheck'">
      <div class="col-md-10 col-lg-10 col-sm-10 col-xs-10">

        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label> Account No</label>
          <input type="number" placeholder="Account No." class="form-control" ng-model="PaymentType.acno">
        </div>
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>Account Holder's Name</label>
          <input type="text" placeholder="Account Holder's Name.." class="form-control" ng-model="PaymentType.acname">
        </div>
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>Account Type</label>
          <select class="form-control" ng-init="PaymentType.actype='checking'" ng-model="PaymentType.actype">
            <option value="checking"> Checking</option>
            <option value="savings"> Savings</option>
          </select>
        </div>
      </div>
      <br>
      <div class="col-md-10 col-lg-10 col-sm-10 col-xs-10" style="float: right;">
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>ABA Routing No.</label>
          <input type="number" placeholder="ABA Routing No." class="form-control" ng-model="PaymentType.aba">
        </div>
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>Bank Name</label>
          <input type="text" placeholder="Bank Name.." class="form-control" ng-model="PaymentType.bank">
        </div>
      </div>
    </div>
  </div>
  <span style="color:red" ng-bind="addPaymentInfo.CCMessage" ng-if="addPaymentInfo.Category=='Online Store'"></span>
  <br>
  <div class="row">
    <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
      <label>Status</label>
      <select ng-model="addPaymentInfo.Status" class="form-control">
        <option ng-option value="Open">Open</option>
        <option ng-option value="Close">Close</option>
        <option ng-option value="Declined">Declined</option>
        <option ng-option value="Processing">Processing</option>
      </select>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
      <label> Order Description <span style="color:black">(in addition to line items)</span> </label>
      <textarea class="form-control" ng-model="addPaymentInfo.description"></textarea>
    </div>
    <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
      <label>Admin Notes</label>
      <textarea ng-model="addPaymentInfo.Notes" class="form-control"></textarea>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
      <button type="button" class="btn btn-color" ng-click="AdditionalMemberPayments(emps,PaymentType,addPaymentInfo)">Save</button>
      <button type="button" class="btn btn-color" ng-click="ExitAdditional()">Exit</button>
    </div>
  </div>
</div>
<div class="panel panel-default" ng-show="isShow" ng-init="Viewmempay()">
