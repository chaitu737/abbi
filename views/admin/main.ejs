<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Main</h2>
</div>
<div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px" ng-hide="eworkorderform2" print-remove ng-init="mainPage()">
  <div class="panel panel-default">
    <div class="panel-body">
      <div class="row">
        <div class="col-md-8"></div>
        <div class="col-md-4">
          <input type="text" class="form-control" placeholder="Search" ng-model="vm.Search" ng-change="searchMain(vm)"/>
        </div>
      </div>
      <div class="table-responsive">
       
        <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
          <thead>
            <tr>
              <th>WO #</th>
              <th>Date</th>
              <th>Member</th>
              <th>Categories</th>
              <th>Assigned By</th>
              <th>Status</th>
            </tr>
          </thead>
          <tbody>
            <tr ng-repeat="mainwo in mainwos">
              <td><a href="#!workorders/{{mainwo.WorkOrderNumber}}/{{mainwo.Categories}}/{{mainwo.status}}" target="_blank"><span ng-bind = "mainwo.WorkOrderNumber"></span></a></td>
              <td><span ng-bind = "mainwo.WorkOrderDate | date : 'MM/dd/yyyy'"></span></td>
              <td><a href="#!memberprofile/{{mainwo.MemberNumber}}" target="_blank"><span ng-bind="mainwo.MemberName"></span></a></td>
              <td><span ng-bind = "mainwo.Categories"></span></td>
              <td><span ng-bind = "mainwo.AssignedBy"></span></td>
              <td><span ng-bind = "mainwo.status"></span></td>
            </tr>
          </tbody>
        </table>
        <div class="dataTables_wrapper no-footer">
          <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
            </div>
            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
        <div class="pagination pagination-centered" ng-show="mainwos[0].xCount" style="margin: 0;">
          <ul class="pagination-controle pagination">
           <li>
            <button type="button" class="btn btn-primary" ng-disabled="curPage == 0"
           ng-click="curPage=curPage-1;mainPage();" style="float: left;"> &lt; PREV</button>
           </li>
           <li>
           <span>Page {{curPage + 1}} of {{ numberOfPages() }}</span>
           </li>
           <li>
           <button type="button" class="btn btn-primary"
           ng-disabled="curPage >= mainwos[0].xCount/pageSize - 1"
           ng-click="curPage = curPage+1;mainPage();" >NEXT &gt;</button>
           </li>
          </ul>
          </div>
          </div>
          </div>
      </div>
    </div>
  </div>
</div>
