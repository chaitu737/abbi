<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Super Stakes</h2>
</div>
<div class="container" style="background-color: #d8ceb9;width:90%;height:100%;padding: 10px;">
  <div class="row"> <br />
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
      <label>Super Stakes Program Year:</label>
      <select ng-model="syear.year" ng-click="StakesYearList()" ng-change="SuperStakesData(syear); ParticipantsSS(syear)">
        <option ng-repeat="syl in syls" value="{{syl.ProgramYear}}">{{syl.ProgramYear}}</option>
      </select>
    </div>
  </div>
  <ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" data-target="#Overview">Overview</a></li>
    <li><a data-toggle="tab" data-target="#Payout">Payout</a></li>
    <li><a data-toggle="tab" data-target="#Participants">Participants</a></li>
  </ul>
  <div class="tab-content">
    <div id="Overview" class="tab-pane fade in active">
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12 form-group">
            <label>Number of Animals Entered: <span style="color:red;" ng-bind="pos.ParticipantCount"></span></label>
          </div>
          <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12 form-group">
            <label>Pot Total: <span style="color:red;" ng-bind="pos.pottotal | currency"></span></label>
          </div>
          <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12 form-group">
            <label>Bull Pot: <span style="color:red;" ng-bind="pos.Bullpot | currency"></span></label>
          </div>
          <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12 form-group">
            <label>Parent Pot (each): <span style="color:red;" ng-bind="pos.Parentpot | currency"></span></label>
          </div>
          <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12 form-group">
            <label>Participating Members: <span style="color:red;" ng-bind="pos.MembersInProgram"></span></label>
          </div>
        </div>
      </div>
    </div>
    <div id="Payout" class="tab-pane fade">
      <div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
        <div class="panel panel-default">
          <div class="panel-body">
            <p style="color: black;">This is the payout amounts for each of the given payout seasons.
              Number of Participants: <span ng-bind="pos.ParticipantCount"></span></p>
              <h3 style="color: black;"> 1 place paid</h3>
              <div class="table-responsive">
                <table class="table table-striped table-bordered nowrap">
                  <thead>
                    <tr>
                      <th> Participant</th>
                      <th> 2Yr Payout(20%)</th>
                      <th> 3Yr Payout(30%)</th>
                      <th> 4Yr Payout(50%)</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td><span>Bull</span></td>
                      <td><span ng-bind="pos.paybull_2yr | currency"></span></td>
                      <td><span ng-bind="pos.paybull_3yr | currency"></span></td>
                      <td><span ng-bind="pos.paybull_4yr | currency"></span></td>
                    </tr>
                    <tr>
                      <td><span>Parent (ea.)</span></td>
                      <td><span ng-bind="pos.paypar_2yr | currency"></span></td>
                      <td><span ng-bind="pos.paypar_3yr | currency"></span></td>
                      <td><span ng-bind="pos.paypar_4yr | currency"></span></td>
                    </tr> 
                  </tbody>
                </table>
              </div>
              <h3 style="color: black;"> 2 place paid</h3>
              <i><h4 style="color: black;">First Place Bull</h4></i>
              <div class="table-responsive">
                <table class="table table-striped table-bordered nowrap">
                  <thead>
                    <tr>
                      <th> Participant</th>
                      <th> 2Yr Payout(20%)</th>
                      <th> 3Yr Payout(30%)</th>
                      <th> 4Yr Payout(50%)</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td><span>Bull</span></td>
                      <td><span ng-bind="pos.pp1paybull_2yr | currency"></span></td>
                      <td><span ng-bind="pos.pp1paybull_3yr | currency"></span></td>
                      <td><span ng-bind="pos.pp1paybull_4yr | currency"></span></td>
                    </tr>
                    <tr>
                      <td><span>Parent (ea.)</span></td>
                      <td><span ng-bind="pos.pp1paypar_2yr | currency"></span></td>
                      <td><span ng-bind="pos.pp1paypar_3yr | currency"></span></td>
                      <td><span ng-bind="pos.pp1paypar_4yr | currency"></span></td>
                    </tr>
                  </tbody>
                </table>
              </div>
              <i><h4 style="color: black;">Second Place Bull</h4></i>
              <div class="table-responsive">
                <table class="table table-striped table-bordered nowrap">
                  <thead>
                    <tr>
                      <th> Participant</th>
                      <th> 2Yr Payout(20%)</th>
                      <th> 3Yr Payout(30%)</th>
                      <th> 4Yr Payout(50%)</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td><span>Bull</span></td>
                      <td><span ng-bind="pos.pp2paybull_2yr | currency"></span></td>
                      <td><span ng-bind="pos.pp2paybull_3yr | currency"></span></td>
                      <td><span ng-bind="pos.pp2paybull_4yr | currency"></span></td>
                    </tr>
                    <tr>
                      <td><span>Parent (ea.)</span></td>
                      <td><span ng-bind="pos.pp2paypar_2yr | currency"></span></td>
                      <td><span ng-bind="pos.pp2paypar_3yr | currency"></span></td>
                      <td><span ng-bind="pos.pp2paypar_4yr | currency"></span></td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div id="Participants" class="tab-pane fade">
        <div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
          <div class="panel panel-default">
            <div class="panel-body">
              <div class="table-responsive">
                <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
                  <thead>
                    <tr>
                      <th>Regno </th>
                      <th>Name </th>
                      <th>Herd#</th>
                      <th>Owner </th>
                      <th>Sire Regno</th>
                      <th>Sire</th>
                      <th>Dam Regno</th>
                      <th>Sire </th>
                      <th>Created </th>
                      <th>PaymentId</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr ng-repeat="dec in Participants">
                      <td><a href="#!editanimals/{{dec.Regno}}" target="_blank"><span ng-bind="dec.Regno"></span></a></td>
                      <td><span ng-bind="dec.Animal"></span></td>
                      <td><span ng-bind="dec.PrivateHerd"></span></td>
                      <td><span ng-bind="dec.MemberName"></span></td>
                      <td><a href="#!editanimals/{{dec.SireRegNo}}" target="_blank"><span ng-bind="dec.SireRegNo"></span></a></td>
                      <td><span ng-bind="dec.Sire"></span></td>
                      <td><a href="#!editanimals/{{dec.DamRegno}}" target="_blank"><span ng-bind="dec.DamRegno"></span></a></td>
                      <td><span ng-bind="dec.Dam"></span></td>
                      <td><span ng-bind="dec.createdate |date:'MM/dd/yyyy'"></span></td>
                      <td><a href="#"><span ng-bind="dec.paymentid"></span></a></td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div id="all" class="tab-pane fade">
        <div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
          <button class="btn btn-color" type="button" style="margin-right:20px;" ng-click="getAll()">View
          Data</button>
          <div class="panel panel-default">
            <div class="panel-body">
              <div class="table-responsive">
                <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
                  <thead>
                    <tr>
                      <th>Reg No#</th>
                      <th>Name</th>
                      <th>Stats(outs, avg mark)</th>
                      <th>Herd#</th>
                      <th>Birth Date</th>
                      <th>Sex</th>
                      <th>Sire Name</th>
                      <th>Dam Name</th>
                      <th>DNA Status</th>
                      <th>Last Updated</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr ng-repeat="al in all">
                      <td>{{al.RegNo}}</td>
                      <td>{{al.Name}}</td>
                      <td>{{al.stat}}</td>
                      <td>{{al.Herd}}</td>
                      <td>{{al.Birthdate |date:'mm/dd/yyyy'}}</td>
                      <td>{{al.Sex}}</td>
                      <td>{{al.Sire_Name}}</td>
                      <td>{{al.Dam_Name}}</td>
                      <td>{{al.DNA_Status}}</td>
                      <td>{{al.Last_Updated}}</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>