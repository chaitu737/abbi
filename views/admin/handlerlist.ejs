<style>
    body {
    font-size: 1.5em;
    line-height: 1.6;
    font-weight: 400;
    font-family: 'Work Sans', sans-serif;
    color: #111111;
    word-wrap: break-word;
    margin: 0;
    background-color: whitesmoke;
}
a:hover{
      color: darkred!important;
      text-decoration: none;
    }
    .darklav {
    background-color: #c6cde3;
}
.blankbar {
    margin: 0;
    padding: 2px 5px 2px 5px;
}
h5 {
    font-size: 2.4rem;
}
table {
    border-collapse: collapse;
    width: 100%;
    margin-top: 0.5em;
}
table {
    display: table;
    text-indent: initial;
    border-spacing: 2px;
    border-color: grey;
}
tbody {
    display: table-row-group;
    vertical-align: middle;
    border-color: inherit;
}
tr {
    display: table-row;
    vertical-align: inherit;
    border-color: inherit;
}
th {
    padding: 3px 6px;
    background-color: lightskyblue;
    border: 1px solid #E1E1E1;
}
th {
    display: table-cell;
    vertical-align: inherit;
    font-weight: bold;
    text-align: -internal-center;
}
td {
    padding: 3px 6px;
    border: 1px solid #E1E1E1;
}
</style>
<div class="container" style="background-color: #dedede!important;box-sizing: border-box;border: 1px solid slategrey;padding: 10px;margin-bottom: 1rem;margin-top: 1rem;
height:100%;width:90%;padding: 10px;height: 90px;">
  <p class="medline menucolor" id="blankbar" >
    <span class="fancyline biggerline" style="font-size: 150%;">ABBI Competition Admin</span><br>
    <a style="color: darkblue; text-decoration: none; background-color: transparent!important; font-size: 17px; " href="http://localhost/admin/home#!/eventhome">home</a> |
     <a style="color: darkblue; text-decoration: none; background-color: transparent!important; font-size: 17px; " href="http://localhost/admin/home#!/pastevent">past events</a> | 
    <a  style="color: darkblue; text-decoration: none; background-color: transparent!important; font-size: 17px;"  href="http://localhost/admin/home#!/addevent">add event</a> |
 <a style="color: darkblue; text-decoration: none; background-color: transparent!important; font-size: 17px; " href="http://localhost/admin/home#!/classlink">Link 2 Classes</a> |
 <a style="color: darkblue; text-decoration: none; background-color: transparent!important; font-size: 17px; " href="http://localhost/admin/home#!/bullboss">animals</a> |
 <a style="color: darkblue; text-decoration: none; background-color: transparent!important; font-size: 17px; " href="http://localhost/admin/home#!/handlerlist">handlers</a> |
 <a style="color: darkblue; text-decoration: none; background-color: transparent!important; font-size: 17px; " href="http://localhost/admin/home#!/program">programs</a></p>
</div>
<div class="container" style="background-color: #f1f1f1 !important;height:100%;width:90%;padding: 0;margin-top: 0;margin-bottom: 12px;">
<h5 class="blankbar darklav">Handlers / Transporters</h5>
</div>
<div class="container" style="background-color: #f1f1f1 !important;height:100%;width:90%;padding: 10px;margin-top: 0;" ng-init="getHandlerList()">
    <a style="background-color:transparent!important;font-size: 17px;color: darkblue;" href="http://localhost/admin/home#!/handlerform" class="blankbar medline"> + Add New Handler</a>
    <table>
        <tbody>
            <tr><th>rowid</th>
        <th>handler</th>
        <th>memno</th>
        <th>phone</th>
        <th>Actions</th>
        </tr>
<tr ng-repeat="obj in handlerArray">
        <td align="center">{{obj.rowid}}</td>
        <td><a>{{obj.handler}}</a></td>
        <td align="center">{{obj.memno}}</td><td align="center">{{obj.phone}}</td>

        <td><button type="button" class="btn btn-light" ng-click="handlerPage(obj)">Edit</button>
        <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#deleteHandler" ng-click="selectId(obj)">Delete</button></td>
    </tr>
       </tbody>
    </table>
</div>

<div class="modal" id="deleteHandler">
    <div class="modal-dialog">
      <div class="modal-content">
  
        <!-- Modal Header -->
        <div class="modal-header" style="padding: 10px;
        text-align: center;">
          <h4 class="modal-title" style="font-size: 22px;">Delete Handler</h4>
        </div>
  
        <!-- Modal body -->
        <div class="modal-body">
          Are you sure you want to delete this Handler?
        </div>
  
        <!-- Modal footer -->
        <div class="modal-footer" style="border:none;padding-top:0">
          <button type="button" class="btn btn-light" data-dismiss="modal">Cancel</button>
          <button type="button" class="btn btn-danger" ng-click="handlerDelete()">Delete</button>
        </div>
      </div>
    </div>
  </div>