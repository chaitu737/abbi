<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Membership Profile Updates</h2>
</div>
<div>
  <div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
      <label>Start Date :</label>
      <input type="date" class="form-control" ng-model-options="{timezone: 'UTC'}" ng-model="memrep.sdate">
      <label>End Date :</label>
      <input type="date" class="form-control" ng-model-options="{timezone: 'UTC'}" ng-model="memrep.edate">
      <label>Member Number</label>
      <input ng-init="updatemember()" class="form-control" type="text" ng-model="memrep.member" list="names">
            <datalist id="names" ng-model="MemberNumber">
              <option ng-repeat="member in members | filter:memrep.member | limitTo:8" value="{{member.MemberNumber}} - {{member.MemberName}}"></option>
            </datalist>
      <button class="btn btn-color" type="button" ng-click="SearchMemProf(memrep)">Search</button>
    </div>
    <br>
    <div>
    <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
      <thead>
        <tr>
          <th>MemberNo </th>
          <th>MemberName </th>  
          <th>Field </th>
          <th>Old_Value </th>
          <th>New_Value</th>
          <th>ModifiedDate</th>
        </tr>
      </thead>
      <tbody>
        <tr ng-repeat="smp in smps">
          <td><span ng-bind="smp.MemberNo"></span></td>
          <td><span ng-bind="smp.MemberName"></span></td>
          <td><span ng-bind="smp.Field"></span></td>
          <td><span ng-bind="smp.Old_Value"></span></td>
          <td><span ng-bind="smp.New_Value"></span></td>
          <td><span ng-bind="smp.ModifiedDate"></span></td>
        </tr>
      </tbody>
    </table>
    </div>
  </div>
</div>