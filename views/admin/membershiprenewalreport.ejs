<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Membership Renewal Report</h2>
</div>
<div>
  <div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
      <label>Start Date :</label>
      <input type="date" ng-model-options="{timezone: 'UTC'}" ng-model="renewalreport.sdate">
      <label>End Date :</label>
      <input type="date" ng-model-options="{timezone: 'UTC'}" ng-model="renewalreport.edate">
      <label>Type:</label>
      <select ng-model="renewalreport.type" ng-click="TypeDropdown()">
        <option value="">All</option>
        <option ng-repeat="td in tds" value="{{td.Type}}">{{td.Type}}</option>
      </select>
      <button class="btn btn-color" type="button" ng-click="SearchReportofRenewal(renewalreport)">Search</button>
    </div>
    <br>
    <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
      <thead>
        <tr>
          <th>PaidDate </th>
          <th>DueDate </th>  
          <th>First Name </th>
          <th>Last Name </th>
          <th>RanchName</th>
          <th>Address</th>
          <th>City</th>
          <th>State</th>
          <th>Zip</th>
          <th>Country</th>
          <th>Type</th>
        </tr>
      </thead>
      <tbody>
        <tr ng-repeat="srfr in srfrs">
          <td><span ng-bind="srfr.PaidDate | date : 'MM/dd/yyyy'"></span></td>
          <td><span ng-bind="srfr.DueDate | date : 'MM/dd/yyyy'"></span></td>
          <td><span ng-bind="srfr.FirstName"></span></td>
          <td><span ng-bind="srfr.LastName"></span></td>
          <td><span ng-bind="srfr.RanchName"></span></td>
          <td><span ng-bind="srfr.Address"></span></td>
          <td><span ng-bind="srfr.City"></span></td>
          <td><span ng-bind="srfr.State"></span></td>
          <td><span ng-bind="srfr.Zip"></span></td>
          <td><span ng-bind="srfr.Country"></span></td>
          <td><span ng-bind="srfr.Type"></span></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>