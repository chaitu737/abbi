<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Report Certificate</h2>
</div>
<div>
  <div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
  <ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" data-target="#certficateprintbatch">Print Batch</a></li>
    <li><a data-toggle="tab" data-target="#certficatesearchanimal12">Search Animal</a></li>
    <li><a data-toggle="tab" data-target="#certficateregborange">RegNo Range</a></li>
    <li><a data-toggle="tab" data-target="#certficateupdaterange">Update Range</a></li>
  </ul>
  <div class="tab-content">
    <div id="certficateprintbatch" class="tab-pane fade in active">
      <div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
        <div class="panel panel-default">
          <div class="panel-body">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" ng-init="CertiCount();CertificateAnimalPrint()">
              <h5>There are <span ng-bind="ccrs.ID"></span> certificates ready to be printed</h5>
            </div>
            <div>
              <button class="btn btn-color"  ng-click="printToCart('printSectionId')">Print</button>
            </div>
          </div>
        </div>
        <div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" id="printSectionId">
          <div class="row col-md-12">
            <div class="col-md-4" ng-repeat="cap in caps">
              <label>Reg No : </label> <label>{{cap.RegNo}}</label>
              <br>
              <label>Animal :</label> <label>{{cap.Animal}}</label> 
              <br>
              <label>Member Number :</label> <label>{{cap.MemberNumber}}</label>
              <br>
              <label>Private Herd :</label> <label>{{cap.PrivateHerd}}</label>
              <br>
              <label>Ranch Name :</label> <label>{{cap.RanchName}}</label>
              <br>
              <label>Birth Date :</label> <label>{{cap.Birthdate | date:'MM-dd-yyyy'}}</label>
              <br>
              <br>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div id="certficatesearchanimal12" class="tab-pane fade">
      <div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
        <input type="text" ng-model="csearch.regno">
        <button class="btn btn-color" placeholder="Enter Reg No." ng-click="SearchCAnimal(csearch)">Search</button>
        <button class="btn btn-color" ng-click="CertificateAnimalPrint()">Print</button>
      </div>
      <br>
      <div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;" ng-click="ViewCertSearchAnimal()">
        <button class="btn btn-color" >View Reports</button>
        <div class="panel panel-default">
          <div class="panel-body">
            <div class="table-responsive">
              <table class="table table-striped table-bordered nowrap" datatable="ng"dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
                <thead>
                  <tr>
                    <th>RegNo</th>
                    <th>Animal</th>
                    <th> Herd #</th>
                    <th>Delete</th>
                  </tr>
                </thead>
                <tbody>
                  <tr ng-repeat="vcs in vcsa">
                    <td><span ng-bind="vcsa.RegNo"></span></td>
                    <td><span ng-bind="vcsa.Animal"></span></td>
                    <td><span ng-bind="vcsa.HerdNumber"></span></td>
                    <td>
                      <i class="fa fa-trash btn btn-danger" ng-click="DeleteCertSearchAnimal(vcsa)"></i>
                    </td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div id="certficateregborange" class="tab-pane fade">
      <div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
        <div class="panel panel-default">
          <div class="panel-body">
            <div class="row">
              <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <label>From:</label>
                <input type="text" ng-model="range.from">
                <label>To:</label>
                <input type="text" ng-model="range.to">
                <button class="btn btn-color" type="button" ng-click="AddRegNoRangeCerti(range)">Add</button>
              </div>
            </div>
            <br>
            <div class="table-responsive">
              <table class="table table-striped table-bordered nowrap" datatable="ng"dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
                <thead>
                  <tr>
                    <th>RegNo</th>
                    <th>Animal</th>
                    <th> Herd #</th>
                    <th>Delete</th>
                  </tr>
                </thead>
                <tr ng-repeat="vcs in vcsa">
                  <td><span ng-bind="vcsa.RegNo"></span></td>
                  <td><span ng-bind="vcsa.Animal"></span></td>
                  <td><span ng-bind="vcsa.HerdNumber"></span></td>
                  <td>
                    <i class="fa fa-trash btn btn-danger" ng-click="DeleteCertSearchAnimal(vcsa)"></i>
                  </td>
                </tr>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div id="certficateupdaterange" class="tab-pane fade">
      <div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
        <div class="panel panel-default">
          <div class="panel-body">
            <div class="row">
              <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <label>From:</label>
                <input type="text" ng-model="certirange.from">
                <label>To:</label>
                <input type="text" ng-model="certirange.to">
              </div>
              <br>
              <br>
              <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <input type="radio" name="Ref" checked value="Ref" ng-model="certirange.ref">
                <label>Update Range with Ref</label>
                <br>
                <input type="radio" name="Ref" value="Brazil" ng-model="certirange.ref">
                <label>Update Brazil</label>
              </div>
              <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <button class="btn btn-color" type="button" ng-click="UpdatePendingPrint(certirange)">Update Pending printing</button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</div>