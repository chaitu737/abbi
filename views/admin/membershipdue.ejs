<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Membership Due</h2>
</div>
<div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" ng-init="ViewMemDue()">
  <div class="row"> <br />
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
      <button class="btn btn-color" type="button" ng-click="SendEmailMemDue(vmds)">Send Email</button>
    </div>
  </div>
  <div class="panel panel-default">
    <div class="panel-body">
      <div class="table-responsive">
        <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
          <thead>
            <tr>
              <th>Member</th>
              <th>Email</th>
              <th>Type</th>
              <th>PaidDate</th>
              <th>DueDate</th>
            </tr>
          </thead>
          <tbody>
            <tr ng-repeat="vmd in vmds">
              <td>{{vmd.MemberName}}</td>
              <td>{{vmd.Email}}</td>
              <td>{{vmd.Type}}</td>
              <td>{{vmd.PaidDate | date:'MM/dd/yyyy'}}</td>
              <td>{{vmd.DueDate | date:'MM/dd/yyyy'}}</td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>    
</div>