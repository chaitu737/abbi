<div class="text-center">
	<h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Letter of Failed Testing</h2>
</div>
<div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
	<ul class="nav nav-tabs">
		<li class="active"><a data-toggle="tab" data-target="#printbatch">Print Batch</a></li>
		<li><a data-toggle="tab" data-target="#searchanimal12">Search Animal</a></li>
		<li><a data-toggle="tab" data-target="#regborange">RegNo Range</a></li>
	</ul>
	<div class="tab-content">
		<div id="printbatch" class="tab-pane fade in active">
			<div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
				<div class="panel panel-default">
					<div class="panel-body">
						<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
							<span class="text-primary" ng-init="failedCount()">There are {{fcount}} "Failed Testing" letter(s) ready to be printed</span>
						</div>
					</div>
				</div>
				<div class="panel panel-default" ng-init="FailedLetter()">
					<div class="panel-body">
						<div class="table-responsive">
							<table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
								<thead>
									<tr>
										<th>Number</th>
										<th>Name</th>
										<th>Print</th>
									</tr>
								</thead>
								<tbody>
									<tr ng-repeat="fletter in fletters">
										<td><span ng-bind="fletter.MemberNumber"></span></td>
										<td><span ng-bind="fletter.MemberName"></span></td>
										<td>
											<i class="fa fa-print btn btn-primary" ng-click="PrintFailedLetter(fletter)"></i>
										</td>
									</tr>
								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div id="searchanimal12" class="tab-pane fade">
			<div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;" ng-init="FailedLetterView()">
				<input type="text" ng-model="letfailsearch.number">
				<button type="submit" class="btn btn-color" placeholder="Enter Register No" ng-click="FailedLetterSearch(letfailsearch)">Search</button>
				<div class="panel panel-default">
					<div class="panel-body">
						<div class="table-responsive">
							<table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
								<thead>
									<tr>
										<th ng-show="hiderow">Member No</th>
										<th>RegNo</th>
										<th>Animal</th>
										<th> Herd #</th>
										<th>Action</th>
									</tr>
								</thead>
								<tbody>
									<tr ng-repeat="flv in flvs">
										<td ng-show="hiderow"><span ng-bind="flv.MemberNo"></span></td>
										<td><span ng-bind="flv.RegNo"></span></td>
										<td><span ng-bind="flv.Animal"></span></td>
										<td><span ng-bind="flv.PrivateHerd"></span></td>
										<td style="white-space: nowrap;">
											<i class="btn btn-primary fa fa-print" ng-click="failedLetterPrints(flv)"></i>
											<i class="btn btn-danger fa fa-trash" ng-click="failedLetterDelete(flv)"></i>
										</td>
									</tr>
								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div id="regborange" class="tab-pane fade">
			<div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
				<div class="panel panel-default">
					<div class="panel-body">
						<div class="row">
							<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
								<label>From:</label>
								<input type="text" ng-model="regnorange.from">
								<label>To:</label>
								<input type="text" ng-model="regnorange.to">
								<button class="btn btn-color" type="button" placeholder="Enter Register No" ng-click="RegNoRange(regnorange)">Search</button>
							</div>
						</div>
						<br>
						<div class="table-responsive" ng-init="FailedLetterRegNoRange()">
							<table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
								<thead>
									<tr>
										<th>RegNo</th>
										<th>Animal</th>
										<th> Herd #</th>
										<th>Delete</th>
									</tr>
								</thead>
								<tbody>
									<tr ng-repeat="rnr in rnrs">
										<td><span ng-bind="rnr.RegNo"></span></td>
										<td><span ng-bind="rnr.Animal"></span></td>
										<td><span ng-bind="rnr.PrivateHerd"></span></td>
										<td>
											<i class="btn btn-danger fa fa-trash" ng-click="RegNoRangeDelete(rnr)"></i>
										</td>
									</tr>
								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>