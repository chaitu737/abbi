<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; width:100%">Edit Animal</h2>
</div>
<div ng-init="EditAnimalsfullData()">
<div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" ng-show="transfer">
  <div>
    <label>Search Owner</label>
    <input class="form-control" type="text" ng-model="searchowner.name">
    <label>Member Number</label>
    <input class="form-control" type="text" ng-model="searchowner.number">
  </div>
  <br>
  <div>
    <button type="button" class="btn btn-color" ng-click="SearchTransferOwner(searchowner,editanimal)">Search</button>
    <button type="button" class="btn btn-color" ng-click="transfer=false;">Exit</button>
  </div>
  <br>
  <div ng-if="stos">
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-striped table-bordered nowrap" width="100%">
            <thead>
              <tr>
                <th>Member Name</th>
                <th>Ranch Name</th>
                <th>Select</th>
              </tr>
            </thead>
            <tbody>
              <tr ng-repeat="sto in stos">
                <td><span ng-bind="sto.MemberName"></span></td>
                <td><span ng-bind="sto.RanchName"></span></td>
                <td>
                  <i class="fa fa-check btn btn-success" ng-click="TransferOwnerName(sto,editanimal)"></i>
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>  

<div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" ng-hide="transfer">
  <div class="row" print-remove>
    <form class="form-group">
      <div class="col-sm-6">
        <div class="col-sm-12 form-group">
          <label>Payment Id: </label>
          <input type="text" class="form-control" ng-model="editanimal.PaymentID">
        </div>
        <div class="col-sm-12 form-group">
          <label>Reg No.</label>
          <label><span ng-bind="editanimal.RegNo"></span></label>
        </div>
