<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Membership Cards</h2>
</div>
<div>
  <div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
          <form>
            <label>From:</label>
            <input type="date" ng-model="msc.from">
            <label>To:</label>
            <input type="date" ng-model="msc.to">
            <button class="btn btn-color" type="submit" ng-click="RangeOfMemberCard(msc)">View Cards</button>
            <button class="btn btn-color" type="submit" ng-click="PrintMemberCard(msc)">Print Cards</button>
          </form>
        </div>
      </div>
    </div>
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
            <thead>
              <tr>
                <th>Member Number</th>
                <th>First Name</th>
                <th>Last Name </th>
                <th>Ranch Name</th>
                <th>Due Date</th>
                <th>Membership Type</th>
              </tr>
            </thead>
            <tbody>
              <tr ng-repeat="mcs in mscs">
                <td><span ng-bind="mcs.MemberNumber"></span></td>
                <td><span ng-bind="mcs.FirstName"></span></td>
                <td><span ng-bind="mcs.LastName"></span></td>
                <td><span ng-bind="mcs.RanchName"></span></td>
                <td><span ng-bind="mcs.DueDate"></span></td>
                <td><span ng-bind="mcs.Type"></span></td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>