<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Work Order</h2>
</div>
<div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" ng-hide="showworkorder" id ="workorderpage">
  <form>
    <div style="background-color: #d8ceb9;height:100%;width:100%;padding: 69px;">
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label> Work Order Number(Any status)</label>
            <input type="text" class="form-control" ng-model="wo.number" value="">
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>Status: </label>
            <select class="form-control" ng-model="wo.status">
              <option selected="selected" value="">Active Work Orders</option>
              <option value="Any">Any Work Order</option>
              <option value="Open">Open</option>
              <option value="Open - Problem">Open - Problem</option>
              <option value="Processed">Processed</option>
              <option value="Processed - Ordered">Processed - Ordered</option>
              <option value="Processed - Problem">Processed - Problem</option>
              <option value="Complete">Complete</option>
              <option value="Closed">Closed</option>
            </select>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>  Assigned To:    </label>
            <select class="form-control" ng-model="wo.assignedto" ng-click="assignedto()">
              <option value="">--Select One--</option>
              <option ng-repeat="assign in assigned" value="{{assign.userid}}">{{assign.UserName}}</option>
            </select>
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12" ng-init="updatemember()">
          <label>Member :</label>
          <input class="form-control" type="text" ng-model="wo.member" list="names">
            <datalist id="names" ng-model="MemberNumber">
              <option ng-repeat="member in members | filter:wo.member | limitTo:8" value="{{member.MemberNumber}} - {{member.MemberName}}"></option>
            </datalist>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>Start Date: </label>
            <input type="date" ng-model-options="{timezone: 'UTC'}" class="form-control" ng-model="wo.sdate" value="">
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>End Date: </label>
            <input type="date" ng-model-options="{timezone: 'UTC'}" class="form-control" ng-model="wo.edate" value="">
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <div class="form-group">
            <label>Contains: </label>
            <select class="form-control" ng-model="wo.contains" ng-click="updatecontains()">
              <option value="">--Select One--</option>
              <option ng-repeat="con in contains" value="{{con.Category}}">{{con.Category}}</option>
            </select>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
          <button class="btn btn-color" type="button" ng-click="SearchWorkOrder(wo)" style="margin-right:20px;">Search</button>
          <button class="btn btn-color" type="button" ng-click="ResetWorkOrder(wo)" style="margin-right:20px;">Reset</button>
          <button class="btn btn-color" ng-if="security1=='3'" type="button" ng-click="AddWorkOrder()" style="margin-right:20px;">Add Work Order</button>
        </div>
      </div>
    </div>
    <div class="panel panel-default" >
      <div class="panel-body">
         <div class="row">
            <div class="col-md-8"></div>
            <div class="col-md-4">
              <input type="text" class="form-control" placeholder="Search" ng-model="vm.Search" ng-change="searchWorkorder(vm)"/>
            </div>
          </div>
        <div class="table-responsive">
         
          <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%" ng-init="SearchWorkOrder1()">
            <thead>
              <tr>
                <th>WO#999</th>             
                <th>WO Date#</th>
                <th>Member</th>
                <th>Contains</th>
                <th>Assigned To</th>
                <th>Assigned By</th>
                <th>Status</th>
                <!-- <th ng-if="security1=='3'">Action</th> -->
              </tr>
            </thead>
            <tbody>
              <tr ng-repeat="wor in wodata | pagination: curPage * pageSize | limitTo: pageSize">
                <td><a href="#!workorders/{{wor.WorkOrderNumber}}/{{wor.Categories}}/{{wor.status}}" target="_blank" ng-bind="wor.WorkOrderNumber"></a></td>
                <td><span ng-bind="wor.WorkOrderDate | date:'MM/dd/yyyy'"></span></td>
                <td><a href="#!memberprofile/{{wor.MemberNumber}}" target="_blank"><span ng-bind="wor.MemberName"></span></a></td>
                <td><span ng-bind="wor.Categories"></span></td>
                <td><span ng-bind="wor.AssignedToName"></span></td>
                <td><span ng-bind="wor.AssignedBy"></span></td>
                <td><span ng-bind="wor.status"></span></td>
                <!-- <td>
                  <i title="Edit" ng-if="security1=='3'" class="btn btn-warning fa fa-edit" ng-click="EditWorkOrder(wor)"></i>
                </td> -->
              </tr>
            </tbody>
          </table>
          <div class="dataTables_wrapper no-footer">
            <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
              </div>
              <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
          <div class="pagination pagination-centered" ng-show="wodata[0].xCount" style="margin: 0;">
            <ul class="pagination-controle pagination">
             <li>
              <button type="button" class="btn btn-primary" ng-disabled="curPage == 0"
             ng-click="curPage=curPage-1;SearchWorkOrder1();" style="float: left;"> &lt; PREV</button>
             </li>
             <li>
             <span>Page {{curPage + 1}} of {{ numberOfPages() }}</span>
             </li>
             <li>
             <button type="button" class="btn btn-primary"
             ng-disabled="curPage >= wodata[0].xCount/pageSize - 1"
             ng-click="curPage = curPage+1;SearchWorkOrder1();" >NEXT &gt;</button>
             </li>
            </ul>
            </div>
            </div>
            </div>
        </div>
      </div>
    </div>
  </form>
</div>
<div class="container" style="height:100%;width:90%;padding: 10px;" ng-show="addworkorder">
  <div class="container edge"style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" ng-hide="wohide1">
    <form>
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="row">
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <div class="form-group">
              <label>Member</label>
              <select class="form-control" ng-model="work.member" ng-init="updatemember()" ng-change="AddMemberWorkorder(work); woshow=true;">
                <option ng-repeat="member in members" value="{{member.MemberNumber}}">{{member.MemberNumber}} - {{member.MemberName}}</option>
              </select>
              <div class="items" ng-show="isSelected">
                <p ng-click="selectItem(item.MemberNumber)" ng-repeat='item in items'>{{item.Membername}}</p>
              </div>
            </div>
          </div>
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <div class="form-group">
              <label>WO Date</label>
              <input type="text" class="form-control" ng-model="currentdate">
            </div>
          </div>
        </div>
      </div>
    </form>
  </div>
  <br>
  <div class="container edge" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" ng-show="woshow">
    <h3 class="text-center text-primary">Billing Information</h3>
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>First Name</label>
          <input type="text" class="form-control" ng-model="workorders.FirstName">
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>Last Name</label>
          <input type="text" class="form-control" ng-model="workorders.LastName">
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>Email</label>
          <input type="text" class="form-control" ng-model="workorders.Email">
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>Phone</label>
          <input type="text" class="form-control" ng-model="workorders.Phone">
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>Membership Type</label>
          <span class="bg-danger" ng-bind="workorders.Type"></span>
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>Membership Due</label>
          <span ng-bind="workorders.DueDate | date: 'MM/dd/yyyy'"></span>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="form-group">
          <label>Address</label>
          <textarea class="form-control" ng-model="workorders.Address"></textarea>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>City</label>
          <input type="text" class="form-control" ng-model="workorders.City">
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>State</label>
          <input type="text" class="form-control" ng-model="workorders.state">
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>Zip Code</label>
          <input type="text" class="form-control" ng-model="workorders.Zip">
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>Country</label>
          <input type="text" ng-model="workorders.Country" class="form-control">
        </div>
      </div>
    </div>
  </div>
  <br>
  <div class="container table-responsive edge" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" ng-show="woshow">
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-bordered table-striped" ng-repeat="head in headings | unique : 'Header'">
            <thead>
              <tr>
                <th width="500px">{{head.Header}}</th>
                <th width="100px">Quantity</th>
                <th width="100px">Price ($)</th>
                <th width="100px">Postage</th>
                <th width="100px">Type</th>
                <th width="100px">Subtotal ($)</th>
              </tr>
            </thead>
            <tbody>
              <tr ng-repeat="worklist in worklists" ng-if="head.Categoryid == worklist.Categoryid">
                <td width="500px">
                  <span ng-bind="worklist.Description"></span>
                </td>
                <td ng-if="worklist.NewBreederOnly==true">
                  <input type="number" class="form-control" ng-blur="shipping(worklist)" ng-change="countQP(worklist,workorders)" ng-model="worklist.Quantity" disabled="">
                </td>
                <td ng-if="worklist.NewBreederOnly==false">
                  <input type="number" class="form-control" ng-blur="shipping(worklist)" ng-change="countQP(worklist,workorders)" ng-model="worklist.Quantity">
                </td>
                <td ng-if="worklist.NewBreederOnly==true">
                  <input type="number" class="form-control" ng-blur="shipping(worklist)" ng-change="countQP(worklist,workorders)" ng-model="worklist.Price" disabled="">
                </td>
                <td ng-if="worklist.NewBreederOnly==false">
                  <input type="number" class="form-control" ng-blur="shipping(worklist)" ng-change="countQP(worklist,workorders)" ng-model="worklist.Price">
                </td>
                <td>
                  <input type="number" class="form-control" ng-blur="shipping(worklist)" ng-change="countQP(worklist,workorders)" ng-model="worklist.ItemPostage"/>
                </td>
                <td ng-if="worklist.ItemPostageType != 'Order' && worklist.ItemPostageType != 'Calculated'">
                  <span ng-bind="worklist.ItemPostageType"></span>
                </td>
                <td ng-if="worklist.ItemPostageType == 'Order'">
                  <span>Calculated</span>
                  <input ng-change="countQP(worklist,workorders)" type="checkbox" ng-model="worklist.checktype">
                </td>
                <td ng-if="worklist.ItemPostageType == 'Calculated'">
                  <span ng-bind="worklist.ItemPostageType"></span>
                  <input type="checkbox" checked="" disabled="">
                </td>
                <td ng-if="worklist.NewBreederOnly==true">
                  <input type="number" class="form-control" ng-model="worklist.SubTotal" disabled="">
                </td>
                <td ng-if="worklist.NewBreederOnly==false">
                  <input type="number" class="form-control" ng-model="worklist.SubTotal">
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-bordered table-striped" ng-if="workorders.Type!='Veterinarian' && workorders.Type!='Lifetime'">
            <thead>
              <tr>
                <th>Membership</th>
                <th>Quantity</th>
                <th>Price ($)</th>
                <th>Postage</th>
                <th>Type</th>
                <th>Subtotal ($)</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>
                  <span>Renew Membership</span>
                </td>
                <td>
                  <input type="checkbox" ng-model="workorders.quantity" checked="" disabled>
                </td>
                <td>
                  <input type="text" class="form-control" ng-model="workorders.MembershipRenewalPrice">
                </td>
                <td></td>
                <td></td>
                <td>
                  <input class="form-control" type="text" ng-model="workorders.MembershipRenewalPrice">
                </td>
              </tr>
              <td>
                <span>Upgrade Membership</span>
              </td>
              <td colspan="4">
                <select ng-model="workorders.Account" ng-change="countQP(worklist,workorders)">
                  <option value="0">--Select Upgrade--</option>
                  <option value="1250">Lifetime Membership Premium Member</option>
                </select>
              </td>
              <td>
                <input class="form-control" type="text" ng-init="workorders.SubTotal1=0" ng-model="workorders.SubTotal1">
              </td>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <div class="row" style="text-align:right; margin-right:15px;">
      <label>Postage Total ($)</label>
      <label  ng-bind="workorders.shippingAmt"></label>
    </div>
    <div class="row" style="text-align:right; margin-right:15px;">
      <label>Sub Total ($)</label>
      <label>{{workorders.TotalPriceAmt - workorders.HandlingAmt}}</label>
    </div>
  </div>
  <br>
  
  <div class="container edge" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" ng-show="woshow2">
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
      <input type="checkbox" ng-model="sameAsBilling" ng-change="sameAsBillingInfo()"/><span style="color: black;padding-left: 6px;">Same as Billing Information</span>
    </div>
    </div>
    <h3 class="text-center text-primary">Shipping Information</h3>
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>First Name</label>
          <input type="text" class="form-control" ng-model="workorders.ShippingFirstName">
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>Last Name</label>
          <input type="text" class="form-control" ng-model="workorders.ShippingLastName">
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>City</label>
          <input type="text" class="form-control" ng-model="workorders.ShippingCity">
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>State</label>
          <input type="text" class="form-control" ng-model="workorders.ShippingState">
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>Zip Code</label>
          <input type="text" class="form-control" ng-model="workorders.ShippingZip">
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>Country</label>
          <input type="text" class="form-control" ng-model="workorders.ShippingCountry">
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group">
          <label>Address</label>
          <textarea class="form-control" rows="5" ng-model="workorders.ShippingAddress"></textarea>
        </div>
      </div>
    </div>
  </div>
