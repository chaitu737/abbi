  <div class="container edge" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;" ng-show="woshow">
    <div class="row">
      <div class="col-sm-12">
        <h3 class="text-center" style="color:#2f2fdc;">Admin</h3>
      </div>
      <div class="col-sm-6 form-group">
        <label>Status</label>
        <select class="form-control" ng-model="workorders.Status" ng-init="workorders.Status='Open'">
          <option ng-value="Open">Open</option>
          <option ng-value="Processed - Problem">Processed - Problem</option>
          <option ng-value="Closed">Closed</option>
          <option ng-value="Open - Problem">Open - Problem</option>
