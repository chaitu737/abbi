</div>
<div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px" ng-show="pay5">
  <div>
    <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
      <div class="row">
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4" style="padding-left:0">
          <label>Sub Total : </label>
        </div>
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <!-- <label>${{workorders.TotalPriceAmt - workorders.HandlingAmt}}</label> -->
          <span style="position: absolute;margin-left: 3px!important;margin-top: 7px!important;color: black;">$</span><input style="padding-left:11px;text-align:left;" type="number" class="form-control" ng-model="workorders.storeTotal" ng-blur="calculateTotal(workorders)"/>
        </div>
      </div>
      <!-- <div class="row">
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4" style="padding-left:0">
          <label>Shipping :</label>
        </div>
        <div class="col-md-2 col-lg-2 col-sm-2 col-xs-2">
          <label ng-bind="workorders.shippingAmt | currency"></label>
        </div>
      </div> -->
      <div class="row" ng-if="PaymentType.Payment !== 'other'">
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4" style="padding-left:0">
          <label>Handling :</label>
        </div>
        <div class="col-md-2 col-lg-2 col-sm-2 col-xs-2">
          <label ng-bind="workorders.HandlingAmt | currency"></label>
        </div>
      </div>
      <div class="row" ng-if="PaymentType.Payment == 'eCheck'">
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4" style="padding-left:0">
          <label>Discount :</label>
        </div>
        <div class="col-md-2 col-lg-2 col-sm-2 col-xs-2">
          <label ng-bind="workorders.HandlingAmt | currency"></label>
        </div>
      </div>
      <div class="row ">
        <div class="col-md-7 col-lg-7 col-sm-7 col-xs-7" style="
        padding: 0;
    ">
          <hr style="
        margin-top: 6px;
        margin-bottom: 6px;
        width: 100%;
    ">
        </div>
      </div>

      <div class="row">
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4" style="padding-left:0">
          <label>Total:</label>
        </div>
        <div class="col-md-2 col-lg-2 col-sm-2 col-xs-2" ng-if="PaymentType.Payment != 'other' && (PaymentType.Payment != 'eCheck' || PaymentType.Payment == '')">
          <label ng-bind="workorders.TotalPriceAmt | currency"></label>
