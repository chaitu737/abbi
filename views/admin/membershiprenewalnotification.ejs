<div class="text-center">
  <h2 style="background-color:#503219; color:#fff; padding:10px; max-width:100%">Membership Renewal Notifications</h2>
</div>
<div>
  <div class="container" style="background-color: #d8ceb9;height:100%;width:90%;padding: 10px;">
	<ul class="nav nav-tabs">
		<li class="active"><a data-toggle="tab" data-target="#printallexpmemb">Print All Expired
		Membership</a></li>
		<li><a data-toggle="tab" data-target="#membershipexpdate">Membership Expiration Date</a></li>
		<li><a data-toggle="tab" data-target="#searchmembernotific">Search Member</a></li>
	</ul>
	<div class="tab-content">
		<div id="printallexpmemb" class="tab-pane fade in active">
			<div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
				<div class="panel panel-default">
					<div class="panel-body">
						<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
							<button class="btn btn-color" ng-click="PrintAllExpired()">Print All Expired</button>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div id="membershipexpdate" class="tab-pane fade">
			<div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
				<div class="panel panel-default">
					<div class="panel-body">
						<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
							<label>Membership Expiration Date:</label>
							<input type="date" ng-model-options="{timezone: 'UTC'}" ng-model="memexp.from">
							<input type="date" ng-model-options="{timezone: 'UTC'}" ng-model="memexp.to">
							<button class="btn btn-color" type="button" ng-click="MemExpiration(memexp)">Search</button>
						</div>
					</div>
				</div>
			</div>
			<br>
			<div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
				<table class="table table-striped table-bordered nowrap" datatable="ng"dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
					<thead>
						<tr>
							<th>Member Number</th>
							<th>Member Name</th>
							<th>Due Date</th>
						</tr>
					</thead>
					<tbody>
						<tr ng-repeat="mse in mses">
							<td><span ng-bind="mse.MemberNumber"></span></td>
							<td><span ng-bind="mse.MemberName"></span></td>
							<td><span ng-bind="mse.DueDate | date:'MM/dd/yyyy'"></span></td>
						</tr>
					</tbody>
				</table>
			</div>
		</div>
		<div id="searchmembernotific" class="tab-pane fade ">
			<div class="container col-md-12" style="background-color: #d8ceb9;height:100%;padding: 10px;">
				<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
					<label>Member Name </label>
					<input type="text" ng-model="renew.name">
					<button class="btn btn-color" type="button" ng-click="MemberRenew(renew)">Search</button>
				</div>
			</div>
			<br>
			<div class="container" style="background-color: #d8ceb9;height:100%;padding: 10px;">
				<table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
					<thead>
						<tr>
							<th>Member Number</th>
							<th>Member Name</th>
							<th>Expiration Date</th>
							<th>Print</th>
						</tr>
					</thead>
					<tbody>
						<tr ng-repeat="mrs in mrss">
							<td><span ng-bind="mrs.MemberNumber"></span></td>
							<td><span ng-bind="mrs.MemberName"></span></td>
							<td><span ng-bind="mrs.DueDate | date : 'MM/dd/yyyy'"></span></td>
							<td>
								<i class="btn btn-info fa fa-print" ng-click="PrintMemRenewal(mrs)"></i>
							</td>
						</tr>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>
</div>