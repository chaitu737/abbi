<div class="container login" ng-hide="showrenewal">
  <h2 class="headerabbilogin" >Membership Renewal Reminder</h2>
  <form>
    <h2 style="margin-top:50px;" class="text-center text-danger"><b><span>Your membership expired on <span ng-bind="duedate"></span></span></b></h2>
    <div style="margin-top:50px;" class="text-center">
      <button class="btn btn-primary" ng-click="showrenewal=true">Renew Membership Now</button>
    </div>
  </form>
</div>
<div class="container login" ng-show="showrenewal">
  <form ng-init="Details()">
    <div class="row col-md-12">
      <div class="col-sm-12">
        <h2 class="headerabbilogin" >Membership</h2>
        <p class="text-danger">Unable to determine Junior Status. Please contact office or choose from membership types below.</p>
        <div class="col-sm-12 form-group">
          <select class="form-control" autocomplete="new-name" ng-model="renew.type" ng-init="memberType1()" required="" ng-change="AmountRenew(renew)">
            <option value="" ng-selected>--Select One--</option>
            <option ng-repeat="membertype in membertypes" value="{{membertype.MembershipPriceID}}">{{membertype.Description}} ({{membertype.Price | currency}} + {{membertype.Handling | currency}} handling) </option>
          </select>
        </div>
        <h2 class="headerabbilogin" >Billing Information</h2>
        <div class="col-sm-12 form-group" ng-hide="true">
          <label>Member Number<span style="color:red;">*</span></label>
          <input type="text" autocomplete="new-name" ng-model="renew.MemberNumber" placeholder="Member Number" class="form-control" ng-required="true" disabled="">
        </div>
        <div class="col-sm-12 form-group">
          <label>First Name<span style="color:red;">*</span></label>
          <input type="text" autocomplete="new-name" ng-model="renew.FirstName" placeholder="First Name" class="form-control" ng-required="true">
        </div>
        <div class="col-sm-12 form-group">
          <label>Last Name<span style="color:red;">*</span></label>
          <input type="text" autocomplete="new-name" ng-model="renew.LastName" placeholder="Last Name" class="form-control" ng-required="true">
        </div>
        <div class="col-sm-12 form-group">
          <label>Email<span style="color:red;">*</span></label>
          <input type="email" autocomplete="new-name" ng-model="renew.Email" placeholder="Email" class="form-control" ng-required="true">
        </div>
        <div class="col-sm-12 form-group">
          <label>Phone<span style="color:red;">*</span></label>
          <input type="text" autocomplete="new-name" ng-model="renew.Phone" placeholder="phone" class="form-control" ng-required="true">
        </div>
        <div class="col-sm-12 form-group">
          <label>Address 1<span style="color:red;">*</span></label>
          <textarea class="form-control" autocomplete="new-name" ng-model="renew.Address" ng-required="true"></textarea>
        </div>
        <div class="col-sm-12 form-group">
          <label>Address 2<span style="color:red;">*</span></label>
          <textarea class="form-control" autocomplete="new-name" ng-model="renew.Address1" ng-required="true"></textarea>
        </div>
        <div class="col-sm-12 form-group">
          <label>City<span style="color:red;">*</span></label>
          <input type="text" autocomplete="new-name" ng-model="renew.City" placeholder="City" class="form-control" ng-required="true">
        </div>
        <div class="col-sm-12 form-group">
          <label>State<span style="color:red;">*</span></label>
          <input type="text" autocomplete="new-name" ng-model="renew.State" placeholder="State" class="form-control" ng-required="true">
        </div>
        <div class="col-sm-12 form-group">
          <label>Country<span style="color:red;">*</span></label>
          <select autocomplete="new-name" ng-model="renew.Country" class="form-control custom-select browser-default" ng-required="true">
            <option value="">- Select a country -</option>
            <option value="US">United States</option>
            <option value="AU">Australia</option>
            <option value="BR">Brazil</option>
            <option value="CA">Canada</option>
            <option value="MX">Mexico</option>
            <option value="NZ">New Zealand</option>
          </select>
        </div>
        <div class="col-sm-12 form-group">
          <label>Zip<span style="color:red;">*</span></label>
          <input type="text" autocomplete="new-name" ng-model="renew.Zip" placeholder="Zip" class="form-control" ng-required="true">
        </div>
      </div>
      <div class="container">
        <div id="Checkout" class="inline">
          <h1>Pay</h1>
          <div class="card-row">
            <span class="visa"></span>
            <span class="mastercard"></span>
            <span class="amex"></span>
            <span class="discover"></span>
          </div>
          <div class="form-group">
            <label>Amount : </label>
            <span ng-bind="amount | currency"></span>
            <input ng-hide="true" class="form-control" ng-model="amount" disabled="" type="text">
          </div>
          <div class="form-group">
            <label for="CreditCardNumber">Card number</label>
            <input id="CreditCardNumber" class="null card-image form-control" ng-model="pay.cc" type="text">
          </div>
          <div class="expiry-date-group form-group">
            <label for="ExpiryDate">Expiry date</label>
            <input id="ExpiryDate" class="form-control" type="text" placeholder="MM / YY" ng-model="pay.expire" maxlength="5">
          </div>
          <div class="security-code-group form-group">
            <label for="SecurityCode">Security code</label>
            <div class="input-container" >
              <input id="SecurityCode" class="form-control" type="text" ng-model="pay.cvv">
              <i id="cvc" class="fa fa-question-circle"></i>
            </div>
            <div class="cvc-preview-container two-card hide">
              <div class="amex-cvc-preview"></div>
              <div class="visa-mc-dis-cvc-preview"></div>
            </div>
          </div>
          <button id="PayButton" class="btn btn-block btn-success submit-button" type="button" ng-click="MembershipRenewal(renew,pay,amount);">
            <span class="submit-button-lock"></span>
          </button>
        </div>
      </div>
    </form>
    <div ng-if="msg.success">
      <div class="payment-success-notice-background">
        <div class="payment-success-notice">
          <div class="payment-success-notice-title">
            <span ng-bind="msg.success"></span>
          </div>
          <div class="payment-success-notice-icon">
            <i class="fa fa-check-circle-o" aria-hidden="true"></i>
          </div>
          <div class="payment-success-notice-paymentamt">
            Total Payment Amount<br/><b><span ng-bind="amount | currency"></span></b>
          </div>
          <p class="text-success"><b>Account Created Successfully</b></p>
          <br/>
          <div id='payment-success-notice-btn' class="payment-success-notice-btn">
            Continue (&nbsp;<span id="payment-success-countdown">5</span>&nbsp;)
          </div>
        </div>
      </div>
    </div>
    <div ng-if="msg.error">
      <div class="payment-danger-notice-background">
        <div class="payment-danger-notice">
          <div class="payment-danger-notice-title">
            <span ng-bind="msg.error"></span>
          </div>
          <div class="payment-danger-notice-icon">
            <i class="fa fa-times-circle-o" aria-hidden="true"></i>
          </div>
          <div class="payment-danger-notice-paymentamt">
            Total Payment Amount<br/><b><span ng-bind="amount | currency"></b>
            </div>
            <br/>
            <div id='payment-danger-notice-btn' class="payment-danger-notice-btn">
              Continue (&nbsp;<span id="payment-danger-countdown">5</span>&nbsp;)
            </div>
          </div>
        </div>
      </div>
      <button type="button" class="btn btn-primary" ng-click="showrenewal=false" style="margin-left:10px;">Exit</button>
    </div>
  </div>
