<div class="container headingforgot" ng-controller="ForgotController">
	<div class="row">
		<div class="col-md-12 col-md-offset-4 forgot">
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="text-center">
            <h3><i class="fa fa-lock fa-2x"></i></h3>
            <h2 class="text-center">Forgot Password?</h2>
            <p>You can reset your password here.</p>
            <div class="panel-body">
              <form>
                <div class="form-group">
                  <div class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-envelope color-blue"></i></span>
                    <input placeholder="Enter Email Address" class="form-control" ng-model="forgot.email" type="email">
                  </div>
                </div>
                <b ng-if="message.message == 'Email Sent Successfully'" ng-style="{color: 'green'}" ng-bind="message.message"></b>
                <b ng-if="message.message == Email Not Found" ng-style="{color: 'red'}" ng-bind="message.message"></b> 
                <div class="form-group">
                  <button class="btn btn-primary" type="submit" ng-click="SendMail(forgot)">
                    Reset Password
                  </button>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
