  <div class="container">
    <div class="row jumbotron signuppage">
      <form name="SignupForm" ng-controller="signupController" ng-submit="UploadImage(signup,PaymentType)" enctype="multipart/form-data">
        <div class="row">
          <div class="col-sm-12">
            <h3 class="text-center" style="color: white;background-color: #503219;">Personal Details</h3>
          </div>
          <div class="col-sm-6 form-group">
            <label>Account Type<span style="color:red;">*</span></label>
            <br>
            <input type="radio" name="accounttype" ng-click="partnerClicked()" autocomplete="new-name" ng-model="signup.personal">Partnership
            <br>
            <input type="radio" name="accounttype" autocomplete="new-name" ng-model="signup.personal">Personal
          </div>
          <div class="col-sm-6 form-group">
            <label>Image<span style="color:red;">*</span></label>
            <div class="imageWrapper">
              <img class="image" src="https://kubalubra.is/wp-content/uploads/2017/11/default-thumbnail.jpg" width="200" height="200">
            </div>
            <br>
            <input class="form-control file-input" type="file" file-model="signup.file">
            <br>
          </div>
          <div class="col-sm-6 form-group">
            <label>First Name<span style="color:red;">*</span></label>
            <input type="text" class="form-control" autocomplete="new-name" ng-model="signup.fname" placeholder="Enter your first name." ng-required="true" onkeypress="return event.charCode != 32">
          </div>
          <div class="col-sm-6 form-group">
            <label>Last Name<span style="color:red;">*</span></label>
            <input type="text" class="form-control" autocomplete="new-name" ng-model="signup.lname" placeholder="Enter your last name." ng-required="true" onkeypress="return event.charCode != 32">
          </div>
          <div class="col-sm-6 form-group">
            <label>Email<span style="color:red;">*</span></label>
            <input type="email" class="form-control" autocomplete="new-name" ng-model="signup.email" placeholder="Enter your email." ng-required="true" onkeypress="return event.charCode != 32">
          </div>
          <div class="col-sm-6 form-group">
            <label>Login Name<span style="color:red;">*</span></label>
            <input type="text" class="form-control" autocomplete="new-name" ng-model="signup.uname" placeholder="Enter your username." ng-blur="Checkusername(signup)" ng-required="true" onkeypress="return event.charCode != 32">
            <b ng-if="usernames==1" ng-style="{color: 'red'}" ng-bind="message"></b>
            <b ng-if="usernames==0" ng-style="{color: 'green'}" ng-bind="message"></b>
          </div>
          <div class="col-sm-6 form-group">
            <label>Ranch<span style="color:red;">*</span></label>
            <input type="text" class="form-control" autocomplete="new-name" ng-model="signup.ranch" placeholder="Enter your ranch." ng-required="true">
          </div>
          <div class="col-sm-6 form-group">
            <label>Membership Type<span style="color:red;">*</span></label>
            <select class="form-control" autocomplete="new-name" ng-model="signup.type" ng-init="memberType()" required="" ng-change="JuniorValidation(signup);Amount(signup)">
              <option value="" ng-selected>--Select One--</option>
              <option ng-repeat="membertype in membertypes" value="{{membertype.MembershipPriceID}}">{{membertype.Description}} ({{membertype.Price | currency}} + {{membertype.Handling | currency}} handling) </option>
            </select>
          </div>
          <div class="col-sm-6 form-group" ng-if="jun=='showdate'">
            <label>Date Of Birth<span style="color:red;">*</span></label>
            <input type="date" id="txtDate" autocomplete="new-name" ng-model="signup.dob" class="form-control" placeholder="" ng-change="JuniorValidation(signup)">
          </div>
          <div class="col-sm-6 form-group">
            <label>USDA Premises ID (PIN)</label>
            <input type="text" autocomplete="new-name" ng-model="signup.usda" class="form-control" placeholder="">
          </div>
          <div class="col-sm-6 form-group">
            <label>Phone(H)<span style="color:red;">*</span></label>
            <input type="tel" autocomplete="new-name" ng-model="signup.mobile" class="form-control" placeholder="xxx-xxx-xxxx" name="phone" ng-minlength="10" ng-maxlength="13" ng-required="true">
            <span style="color:red" ng-show="SignupForm.phone.$error.minlength">Enter Valid Phone Number</span>
            <span style="color:red" ng-show="SignupForm.phone.$error.maxlength">Enter Valid Phone Number</span>
          </div>
          <div class="col-sm-6 form-group">
            <label>Fax</label>
            <input type="text" autocomplete="new-name" ng-model="signup.fax" class="form-control" placeholder="Enter your Fax No.">
          </div>
          <div class="col-sm-6 form-group">
            <label>Password<span style="color:red;">*</span></label>
            <input type="Password" name="pass" autocomplete="new-name" ng-model="signup.passwd" class="form-control" placeholder="Enter your password." ng-required="true" ng-minlength="6">
            <span style="color:red" ng-show="SignupForm.pass.$error.minlength">Password length should be atleast 6</span>
          </div>
          <div class="col-sm-6 form-group">
            <label>Confirm Password<span style="color:red;">*</span></label>
            <input type="Password" autocomplete="new-name" ng-model="signup.passwd1" class="form-control" placeholder="Re-enter your password." ng-blur="checkPassword(signup)" ng-required="true">
            <b style="color:red" ng-bind="changemessage"></b>
          </div>
        </div>
        <div class="row col-md-12">
          <div class="col-sm-6">
            <div class="col-sm-12">
              <label>Contact Address</label>
            </div>
            <div class="col-sm-12 form-group">
              <label>Address<span style="color:red;">*</span></label>
              <input type="text" autocomplete="new-name" ng-model="signup.txtaddress" placeholder="Address" class="form-control" ng-required="true">
            </div>
            <div class="col-sm-12 form-group">
              <label>City<span style="color:red;">*</span></label>
              <input type="text" autocomplete="new-name" ng-model="signup.txtcity" placeholder="City" class="form-control" ng-required="true">
            </div>
            <div class="col-sm-12 form-group">
              <label>State<span style="color:red;">*</span></label>
              <input type="text" autocomplete="new-name" ng-model="signup.txtstate" placeholder="State" class="form-control" ng-required="true">
            </div>
            <div class="col-sm-12 form-group">
              <label>Country<span style="color:red;">*</span></label>
              <select autocomplete="new-name" ng-model="signup.ccode" class="form-control custom-select browser-default" ng-required="true">
                <option value="">- Select a country -</option>
                <option value="US">United States</option>
                <option value="AU">Australia</option>
                <option value="BR">Brazil</option>
                <option value="CA">Canada</option>
                <option value="MX">Mexico</option>
                <option value="NZ">New Zealand</option>
              </select>
            </div>
            <div class="col-sm-12 form-group">
              <label>Zip<span style="color:red;">*</span></label>
              <input type="text" autocomplete="new-name" ng-model="signup.zip" placeholder="Zip Code" class="form-control" ng-required="true">
            </div>
          </div>
          <div class="col-sm-6">
            <div class="col-sm-12">
              <label>Billing Address(must match credit card address)</label><input class="form-check d-inline" type="checkbox" value="" ng-change="SameAddress()" autocomplete="new-name" ng-model="sameasabove">
            </div>
            <div class="col-sm-12 form-group">
              <label>Address<span style="color:red;">*</span></label>
              <input type="text" autocomplete="new-name" ng-model="signup.txtaddress1" placeholder="Address" class="form-control" ng-required="true">
            </div>
            <div class="col-sm-12 form-group">
              <label>City<span style="color:red;">*</span></label>
              <input type="text" autocomplete="new-name" ng-model="signup.txtcity1" placeholder="City" class="form-control" ng-required="true">
            </div>
            <div class="col-sm-12 form-group">
              <label>State<span style="color:red;">*</span></label>
              <input type="text" autocomplete="new-name" ng-model="signup.txtstate1" placeholder="State" class="form-control" ng-required="true">
            </div>
            <div class="col-sm-12 form-group">
              <label>Country<span style="color:red;">*</span></label>
              <select autocomplete="new-name" ng-model="signup.ccode1 " class="form-control custom-select browser-default" ng-required="true">
                <option value="">- Select a country -</option>
                <option value="US">United States</option>
                <option value="AU">Australia</option>
                <option value="BR">Brazil</option>
                <option value="CA">Canada</option>
                <option value="MX">Mexico</option>
                <option value="NZ">New Zealand</option>
              </select>
            </div>
            <div class="col-sm-12 form-group">
              <label>Zip<span style="color:red;">*</span></label>
              <input type="text" autocomplete="new-name" ng-model="signup.zip1" placeholder="Zip Code" class="form-control" ng-required="true"> 
              <!-- onkeypress="return (event.charCode >= 48 && event.charCode <= 57)" -->
            </div>
          </div>
          <div class="col-md-6 col-lg-6 col-sm-6 col-xs-6">
          <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
            <div class="row">
              <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
                <label>Membership Total :</label>
              </div>
              <div class="col-md-2 col-lg-2 col-sm-2 col-xs-2">
                <label ng-bind="amount1 | currency"></label>
              </div>
            </div>
            <div class="row">
              <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
                <label>Handling :</label>
              </div>
              <div class="col-md-2 col-lg-2 col-sm-2 col-xs-2">
                <label ng-bind="handling | currency"></label>
              </div>
            </div>
            <div class="row" ng-if="PaymentType.Payment == 'eCheck'">
              <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
                <label>Discount :</label>
              </div>
              <div class="col-md-2 col-lg-2 col-sm-2 col-xs-2">
                <label ng-bind="handling | currency"></label>
              </div>
            </div>
            <div class="row ">
              <div class="col-md-7 col-lg-7 col-sm-7 col-xs-7" style="
              padding: 0;
          ">
                <hr style="
              margin-top: 6px;
              margin-bottom: 6px;
              width: 100%;
          ">
              </div>
            </div>
      
            <div class="row">
              <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4" style="padding-bottom: 12px;">
                <label>Total :</label>
              </div>
              <div class="col-md-2 col-lg-2 col-sm-2 col-xs-2">
                <label ng-bind="signup.amount | currency"></label>
              </div>
            </div>
      
         </div>
          </div>
          <div class="col-md-6 col-lg-6 col-sm-6 col-xs-6">
            <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
            <label>Payment Type</label>
            <select name="type" ng-model="PaymentType.Payment" ng-change="CardDetails(PaymentType);Amount(signup)" class="form-control">
              <option ng-option selected="selected" value="">-Select One-</option>
              <option ng-option value="VI">Visa</option>
              <option ng-option value="MC">Master Card</option>
              <option ng-option value="AMEX">American Express</option>
              <option ng-option value="Discover">Discover</option>
              <!-- <option ng-option value="Auth.Net">Authorize.Net</option> -->
              <!-- <option ng-option value="Check">Check</option> -->
              <option ng-option value="eCheck">e-Check</option>
              <!-- <option ng-option value="Money Order">Money Order</option> -->
              <!-- <option ng-option value="Cashier Check">Cashier Check</option> -->
            </select>
          </div>
          </div>
          <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12" style="padding-bottom: 14px;">
    <div
      ng-if="PaymentType.Payment == 'undefined' ||PaymentType.Payment == '-1' || PaymentType.Payment == 'VI' || PaymentType.Payment == 'MC' || PaymentType.Payment == 'AMEX' || PaymentType.Payment == 'Discover' || PaymentType.Payment == 'Check' ||  PaymentType.Payment == 'Money Order' || PaymentType.Payment == 'Cashier Check'">
      <div class="row col-md-10 col-lg-10 col-sm-10 col-xs-10">
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Credit Card No</label>
          <input type="number" ng-model="PaymentType.cc" required placeholder="Credit Card No." class="form-control">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Exp. (MM/YY)</label>
          <input type="text" ng-model="PaymentType.expire" required placeholder="Exp. (MM/YY)" class="form-control">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Sec Code</label>
          <input type="number" ng-model="PaymentType.cvv" required placeholder="Sec Code" class="form-control">
        </div>
        <div ng-if="PaymentType.cc!=null" class="col-md-3 col-lg-3 col-sm-3 col-xs-3" style="padding-top: 27px;">
          <i class="fa fa-trash btn btn-danger" ng-click="DeleteCard()"></i>
        </div>
      </div>
    </div>
    <div ng-if="PaymentType.Payment == 'Auth.Net'">
      <div class="col-md-10 col-lg-10 col-sm-10 col-xs-10">
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Credit Card No</label>
          <input type="number" ng-model="PaymentType.cc" required placeholder="Credit Card No." class="form-control">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Exp. (MM/YY)</label>
          <input type="text" ng-model="PaymentType.expire" required placeholder="Exp. (MM/YY)" class="form-control">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Sec Code</label>
          <input type="number" ng-model="PaymentType.cvv" required placeholder="Sec Code" class="form-control">
        </div>
        <div class="col-md-3 col-lg-3 col-sm-3 col-xs-3">
          <label>Auth No.</label>
          <input type="number" ng-model="PaymentType.auth" required placeholder="Auth No." class="form-control">
        </div>
      </div>
    </div>
    <div ng-if="PaymentType.Payment == 'eCheck'">
      <div class="row col-md-10 col-lg-10 col-sm-10 col-xs-10">

        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label> Account No</label>
          <input type="number" ng-model="PaymentType.acno" required placeholder="Account No." class="form-control">
        </div>
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>Account Holder's Name</label>
          <input type="text" ng-model="PaymentType.acname" required placeholder="Account Holder's Name.."
            class="form-control">
        </div>
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>Account Type</label>
          <select ng-model="PaymentType.actype" required class="form-control">
            <option value="checking"> Checking</option>
            <option value="savings"> Savings</option>
          </select>
        </div>
      </div>
      <br>
      <div class="row col-md-10 col-lg-10 col-sm-10 col-xs-10" style="float: right;">
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>ABA Routing No.</label>
          <input type="number" ng-model="PaymentType.aba" required placeholder="ABA Routing No." class="form-control">
        </div>
        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
          <label>Bank Name</label>
          <input type="text" ng-model="PaymentType.bank" required placeholder="Bank Name.." class="form-control">
        </div>
      </div>
    </div>
    </div>
 <br/><br/>
          <div class="col-sm-12 form-group mb-0" ng-init="dontshow">
            <button type="reset" class="btn btn-danger" ng-init="usernames=1">Reset</button>
