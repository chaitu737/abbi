<div class="container border" style="height:100%;width:90%;padding: 10px;" ng-show="showani" print-remove>
  <div class="col-md-12">
    <div class="col-md-6">
      <label>Member : </label>
      <p class="text-primary" ng-bind="memberprofile.MemberName"></p>
      <label>Reg No : </label>  
      <p class="text-primary" ng-bind="memberprofile.RegNo"></p>
      <label>Name : </label>  : </b>
      <p class="text-primary" ng-bind="memberprofile.Animal"></p>
      <label>Lifetime Earnings :</label> 
      <p class="text-primary" ng-bind="lifetime.LifeTime_Earnings | currency"></p>
      <label>Sex :</label>  
      <p class="text-primary" ng-bind="memberprofile.TypeService"></p>
      <label>Private Herd Number :</label> 
      <p class="text-primary" ng-bind="memberprofile.PrivateHerd"></p>
      <label>Birthdate : </label> 
      <p class="text-primary" ng-bind="memberprofile.BirthDate | date : 'MM/dd/yyyy'"></p>
      <label>Description : </label> 
      <p class="text-primary" ng-bind="memberprofile.Description"></p>
      <label>Horns : </label> 
      <p class="text-primary" ng-bind="memberprofile.Horns"></p>
      <label>Original Breeder :</label> 
      <p class="text-primary" ng-bind="memberprofile.OriginalBreeder"></p>
    </div>
    <div class="col-md-6" ng-if="memberprofile.Status == 'Complete' || memberprofile.Status == 'Waiting For Samples'">
      <div>
        <button type="button" class="btn btn-color" ng-click="SoldAnimal(memberprofile)">Mark Animal As Sold</button>
        <button type="button" class="btn btn-color" ng-click="DeceasedAnimal(memberprofile)">Mark Animal As Deceased</button>
        <button ng-if="memberprofile.sold!=true" type="button" class="btn btn-color" ng-click="TransferAnimal(memberprofile)">Transfer Animal</button>
      </div>
      <br>
      <div>
        <div ng-hide="showtext">
          <b style="color:black">Notes</b>
          <textarea class="form-control" ng-bind="notes.Notes" ng-model="notes.Notes" rows="5" disabled="true"></textarea>
        </div>
        <div ng-show="showtext">
          <b style="color:black">Notes</b>
          <textarea class="form-control" ng-bind="notes.Notes" ng-model="notes.Notes" rows="5"></textarea>
        </div>
        <br>
        <div class="text-left">
          <button type="button" class="btn btn-color" ng-click="SaveNotesInv(notes,memberprofile);ShowText()">Save</button>
          <button class="btn btn-color" ng-click="EditText();">Edit</button>
          <button class="btn btn-color" data-toggle="modal" data-target="#adddna">Additional DNA</button>
          <button class="btn btn-color" data-toggle="modal" data-target="#addeid">Order EID</button>
        </div>
      </div>
      <div class="modal fade" id="adddna" role="dialog">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal">&times;</button>
              <h4 class="modal-title">Additional DNA</h4>
            </div>
            <div class="modal-body">
              <div class="row col-md-12">
                <div class="col-md-6">
                  <form ng-submit="AddDNAPay(dna,PaymentType)">
                    <label>DNA :</label>
                    <input class="form-control" type="text" ng-model="dna.dnaname" ng-required="true">
                    <label>Ran Aganist :</label>
                    <input class="form-control" type="text" ng-model="dna.ran" ng-required="true">
                  </div>
                  <div class="col-md-6">
                    <label>Payment Type</label>
                    <select name="type" ng-model="PaymentType.Payment" class="form-control" ng-required="true">
                      <option ng-option selected="selected" value="">-Select One-</option>
                      <option ng-option value="VI">Visa</option>
                      <option ng-option value="MC">Master Card</option>
                      <option ng-option value="AMEX">American Express</option>
                      <option ng-option value="Discover">Discover</option>
                      <!-- <option ng-option value="Auth.Net">Authorize.Net</option> -->
                      <!-- <option ng-option value="Check">Check</option> -->
                      <option ng-option value="eCheck">e-Check</option>
                      <!-- <option ng-option value="Money Order">Money Order</option> -->
                      <!-- <option ng-option value="Cashier Check">Cashier Check</option> -->
                    </select>
                  </div>
                  <br>
                  <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12"></div>
                    <div ng-if="PaymentType.Payment == 'undefined' ||PaymentType.Payment == '-1' || PaymentType.Payment == 'VI' || PaymentType.Payment == 'MC' || PaymentType.Payment == 'AMEX' || PaymentType.Payment == 'Discover' || PaymentType.Payment == 'Check' ||  PaymentType.Payment == 'Money Order' || PaymentType.Payment == 'Cashier Check'">
                      <div class="col-md-10 col-lg-10 col-sm-10 col-xs-10">
                        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
                          <label>Credit Card No</label>
                          <input type="number" ng-model="PaymentType.cc" required placeholder="Credit Card No." class="form-control">
                        </div>
                        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
                          <label>Exp. (MM/YY)</label>
                          <input type="text" ng-model="PaymentType.expire" required placeholder="Exp. (MM/YY)" class="form-control">
                        </div>
