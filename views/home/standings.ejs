<div ng-show="tablestandings" class="container" style="height:100%;width:90%;padding: 10px;">
  <table class="table table-striped table-bordered nowrap" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance" width="100%">
    <thead>
      <tr>
        <th>mpos</th>
        <th>name</th>
        <th>owner</th>
        <th>money</th>
        <th>ttl pts</th>
        <th>avg</th>
        <th>outs</th>
      </tr>
    </thead>
    <tbody>
      <tr ng-repeat="season in seasons">
        <td ng-bind="season.mpos"></td>
        <td ng-bind="season.name"></td>
        <td ng-bind="season.owner"></td>
        <td ng-bind="season.money"></td>
        <td ng-bind="season.ttl pts"></td>
        <td ng-bind="season.avg"></td>
        <td ng-bind="season.outs"></td>
      </tr>
    </tbody>
  </table>
  <br>
  <button class="btn btn-color" ng-click="ExitTableCurrentStand()">Exit</button>
</div>
