<div>
  <h2 class="text-center" style="background-color:#503219; color:#fff; padding:5px; min-width:100px;">Holding Pen</h2>
</div>
<div class="container border" style="height:100%;width:90%;padding: 10px;" ng-init="ShowRegAnimalData()">
  <!-- default screen in holding pen -->
  <div ng-hide="showcheckout">
    <button type="button" class="btn btn-color" ng-click="redirectTo()">Add Animal</button>
    <button type="button" class="btn btn-color" ng-click="ShowCheckOut();EidInfo();ShowNextPanel()">Checkout</button>
    <br>
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-bordered table-striped table-hover">
            <thead>
              <tr>
                <th ng-show="false">RegNo</th>
                <th>Checkout</th>
                <th>Animal</th>
                <th>Birthdate</th>
                <th>Herd#</th>
                <th>Sex</th>
                <th>Action</th>
              </tr>
            </thead>
            <tbody>
              <tr ng-repeat="anidetails in animdetails">
                <td ng-show="false"><span ng-bind="anidetails.RegNoID"></span></td>
                <td>
                  <input type="checkbox" ng-model="anidetails.Completed" ng-true-value="{{ 0 | json }}" ng-false-value="{{ 2 | json }}" ng-change="CheckHoldingPen(anidetails)">
                </td>
                <td><span ng-bind="anidetails.Animal"></span></td>
                <td><span ng-bind="anidetails.Birthdate | date : 'MM/dd/yyyy'"></span></td>
                <td><span ng-bind="anidetails.PrivateHerd"></span></td>
                <td><span ng-bind="anidetails.TypeService"></span></td>
                <td style="white-space: nowrap;">
                  <i title="Edit" class="btn btn-warning fa fa-edit" ng-click="editHoldingPen(anidetails); ShowListInNextPage(anidetails);showpenClick()"></i>
                  <i title="Delete" class="btn btn-danger fa fa-trash" ng-click="RemoveHoldingPen(anidetails)"></i>
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  <!-- edit animal screen -->
  <div class="container" style="height:100%;width:90%;padding: 10px;" ng-show="showpen">
    <div class="row col-md-12" ng-show="onee">
      <form name="UpdateAnimal" ng-submit="UpdateAnimalDetails(reganimal)">
        <div class="row">
          <div class="row">
            <h3 class="text-center text-primary">Animal Details</h3>
          </div> 
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
              <label class="profile_details_text"> Animal Photo</label><br/>
              <img src ="../../../uploads/animals/{{reganimal.picture}}" style="width:161px;">
              <div style="padding-top: 20px;" ng-if="posterTitle">
                <label>{{posterTitle}}</label><span style="font-size: 18px;font-weight: bold;color: red;border: 1px solid black;padding: 0px 2px;margin-left: 10px;border-radius: 4px;cursor: pointer;" ng-click="removeImage1()">x</span>
              </div>
              <br/>
              <input type="file" class="form-control file-input" file-model="reganimal.image" style="background-color: #d8ceb9;">
            </div>
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <div class="form-group">
              <label class="profile_details_text"> Animal Name</label>
              <input type="text" ng-keyup="CheckAnimal(reganimal)" ng-model="reganimal.ANIMAL" class="form-control" value="" ng-required="true" autocomplete="new-name">
            </div>
            <b ng-if="animalname>0" ng-style="{color: 'red'}" ng-bind="message"></b>
            <b ng-if="animalname==0" ng-style="{color: 'green'}" ng-bind="message"></b>
          </div>
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 ">
            <div class="form-group">
              <label class="profile_details_text">Birthdate</label>
              <b><span ng-bind="reganimal.unBd" ng-hide="showdate" ng-click="showdate=true" class="form-control"></span></b>
              <input type="text"  ng-model="reganimal.unBd" ng-show="showdate" class="form-control" valueAsDate="{{reganimal.unBd}}">
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 ">
            <div class="form-group">
              <label class="profile_details_text">Sex</label>
              <select class="form-control" ng-model="reganimal.TypeService" ng-required="true" autocomplete="new-name">
                <option value="">--Select Sex--</option>
                <option value="NB">NB - Natural Bull</option>
                <option value="NC">NC - Natural Cow</option>
                <option value="AB">AB - AI Bull</option>
                <option value="AC">AC - AI Cow</option> 
                <option value="EB">EB - Embryo Bull</option>
                <option value="EC">EC - Embryo Cow</option>
                <option value="NS">NS - Natural Stallion</option>
                <option value="NM">NM - Natural Mare</option>
                <option value="NG">NG - Natural Gelding</option>
                <option value="AS">AS - AI Stallion</option>
                <option value="AM">AM - AI Mare</option>
                <option value="AG">AG - AI Gelding</option>
                <option value="ES">ES - Embryo Stallion</option>
                <option value="EM">EM - Embryo Mare</option>
                <option value="EG">EG - Embryo Gelding</option>
              </select>
            </div>
          </div>
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 ">
            <div class="form-group">
              <label class="profile_details_text">Animal Identification No.(Brand No.).</label>
              <input type="text" ng-blur="CheckHerd(reganimal)" ng-model="reganimal.PrivateHerd" class="form-control" name="herdno" value="" ng-required="true" autocomplete="new-name" maxlength="6">
            </div>
            <b ng-if="herd>0" ng-style="{color: 'red'}" ng-bind="hmessage"></b>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 ">
            <div class="form-group">
              <label class="profile_details_text">Horns</label>
              <select class="form-control" ng-model="reganimal.Horns" ng-required="true" autocomplete="new-name">
                <option value="">--Select Horns--</option>
                <option value="H">Horned</option>
                <option value="D">Dehorned</option>
                <option value="P">Polled</option>
              </select>
            </div>
          </div>
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <label>Main Color</label>
            <select class="form-control" ng-model="reganimal.Main_colour" ng-required="true">
              <option value="">--Select One--</option>
              <option value="black">black</option>
              <option value="white">white</option>
              <option value="grey">grey</option>
              <option value="yellow">yellow</option>
              <option value="cream">cream</option>
              <option value="orange">orange</option>
              <option value="blue">blue</option>
              <option value="red">red</option>
              <option value="brown">brown</option>
            </select>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <label>Secondary Color</label>
            <select class="form-control" ng-model="reganimal.Secondary_colour">
              <option value="">--Select One--</option>
              <option value="black">black</option>
              <option value="white">white</option>
              <option value="grey">grey</option>
              <option value="yellow">yellow</option>
              <option value="cream">cream</option>
              <option value="orange">orange</option>
              <option value="blue">blue</option>
              <option value="red">red</option>
              <option value="brown">brown</option>
            </select>
          </div>
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <label>Pattern</label>
            <select class="form-control"  ng-model="reganimal.pattern">
              <option value="">--Select One--</option>
              <option value="spotted">spotted</option>
              <option value="paint">paint</option>
              <option value="brindle">brindle</option>
              <option value="roan">roan</option>
              <option value="speckled">speckled</option>
            </select>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <label>Additional Markings</label>
            <select class="form-control" ng-model="reganimal.markings">
              <option value="">--Select One--</option>
              <option value="blaze on face">blaze on face</option>
              <option value="diamond on face">diamond on face</option>
              <option value="rings around eyes">rings around eyes</option>
              <option value="sock">sock</option>
              <option value="socks">socks</option>
              <option value="white tail tip">white tail tip</option>
              <option value="black ears">black ears</option>
            </select>
          </div>
          <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
            <div class="form-group">
              <label class="profile_details_text">Description</label>
              <textarea class="form-control" ng-model="reganimal.Description" ng-readonly="true"></textarea>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="form-group">
              <input type="radio" ng-model="reganimal.regincludingdna" checked="" ng-true-value="{{ 1 | json }}" ng-false-value="{{ 0 | json }}">
              <label class="profile_details_text">Registration-Including DNA Testing</label>
            </div>
          </div>
        </div>
        <button type="submit" class="btn btn-color">Next</button>
      </form>
    </div>
    <div class="row col-md-12" ng-show="two">
      <!-- <hr> -->
      <div class="row">
        <div class="row">
          <h3 class="text-center text-primary">Sire Details</h3>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
          <input type="radio" name="sire"  ng-click="option='sregno'">
          <label >Enter the registration number for each sire</label><br>
          <input type="radio"  name="sire" ng-click="option='ssire';currentSireClick()">
          <label >Select sires from your herd</label><br>
          <input type="radio"  name="sire" ng-click="option='ssomeone'">
          <label >Select sires from someone else's herd</label>
        </div>
      </div>
      <div ng-show="option ==='sregno'">
        <div class="col-md-12 row">
          <div class="col-md-6" style="padding-top: 10px;">
            <input type="number" class="form-control" placeholder="Enter Registration Number" ng-model="reganimal.txtreg">
          </div>
          <div class="col-md-6" style="padding-top: 11px;">
            <button type="button" class="btn btn-color" ng-click="addRegNoSire(reganimal)">Add</button>
          </div>
        </div>
      </div>
      <div ng-show="option ==='ssire'">
        <div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" style="padding-top: 20px;">
            <ul class="nav nav-tabs">
              <li  class="s" ng-class="{'active': editsire =='a'}" ng-click="editsire = 'a';currentSireClick()"><a >Currrent</a></li>
              <li  class="s" ng-class="{'active': editsire =='b'}" ng-click="editsire = 'b';previousSireClick()"><a >Previous </a></li>
              <li  class="s" ng-class="{'active': editsire =='c'}" ng-click="editsire = 'c';deceasedSireClick()"><a >Deceased </a></li>
              <li  class="s" ng-class="{'active': editsire =='d'}" ng-click="editsire = 'd';currentbreedingSireClick()"><a >Current Breeding </a></li>
            </ul>
            <div class="tab-content">
              <div id="currentsire" class="tab-pane fade in" ng-class="{'active': editsire =='a'}">
                <div class="container edge col-md-12" style="height:100%;width: 100%;margin-top: 0;padding: 0;">
                  <!-- <button class="btn btn-color"  ng-click="currentSireClick()">View Animal</button> -->
                  <div class="panel panel-default">
                    <div class="panel-body">
                      <div class="table-responsive">
                        <table class="table table-bordered table-striped table-hover" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance"  width="100%">
                          <thead>
                            <tr>
                              <th>RegNo</th>
                              <th>Animal</th>
                              <th>Add To Current Breeding List</th>
                              <th>Action</th>
                            </tr>
                          </thead>
                          <tbody>
                            <tr ng-repeat="cs in css">
                              <td><span ng-bind="cs.RegNo"></span></td>
                              <td><span ng-bind="cs.Animal"></span></td>
                              <td>
                                <i class="fa fa-plus btn btn-success" ng-click="AddCurrentSireToBreeding(cs,reganimal)"></i>
                              </td>
                              <td>
                                <i class="fa fa-plus btn btn-success" ng-click="AddCurrentSireToList(cs,reganimal)"></i>
                              </td>
                            </tr>
                          </tbody>
                        </table>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div id="previoussire" class="tab-pane fade in" ng-class="{'active': editsire =='b'}">
                <div class="container col-md-12" style="height:100%;width: 100%;margin-top: 0;padding: 0;">
                  <!-- <button class="btn btn-color"  ng-click="previousSireClick()">View Sire</button> -->
                  <div class="panel panel-default">
                    <div class="panel-body">
                      <div class="table-responsive">
                        <table class="table table-bordered table-striped table-hover" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance"  width="100%">
                          <thead>
                            <tr>
                              <th>RegNo</th>
                              <th>Animal</th>
                              <th>Owner</th>
                              <th>Action</th>
                            </tr>
                          </thead>
                          <tbody>
                            <tr ng-repeat="ps in pss">
                              <td><span ng-bind="ps.RegNo"></span></td>
                              <td><span ng-bind="ps.Animal"></span></td>
                              <td><span ng-bind="ps.MemberName"></span></td>
                              <td>
                                <i class="fa fa-plus btn btn-success" ng-click="AddPreviousSireToList(ps,reganimal)"></i>
                              </td>
                            </tr>
                          </tbody>
                        </table>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div id="deceasedsire" class="tab-pane fade in" ng-class="{'active': editsire =='c'}">
                <div class="container col-md-12" style="height:100%;width: 100%;margin-top: 0;padding: 0;">
                  <!-- <button class="btn btn-color"  ng-click="deceasedSireClick()">View Sire</button> -->
                  <div class="panel panel-default">
                    <div class="panel-body">
                      <div class="table-responsive">
                        <table class="table table-bordered table-striped table-hover" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance"  width="100%">
                          <thead>
                            <tr>
                              <th>RegNo</th>
                              <th>Animal</th>
                              <th>Action</th>
                            </tr>
                          </thead>
                          <tbody>
                            <tr ng-repeat="ds in dss">
                              <td><span ng-bind="ds.RegNo"></span></td>
                              <td><span ng-bind="ds.Animal"></span></td>
                              <td>
                                <i class="fa fa-plus btn btn-success" ng-click="AddDeceasedSireToList(ds,reganimal)"></i>
                              </td>
                            </tr>
                          </tbody>
                        </table>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div id="currentbreedingsire" class="tab-pane fade in" ng-class="{'active': editsire =='d'}">
                <div class="container col-md-12" style="height:100%;width: 100%;margin-top: 0;padding: 0;">
                  <!-- <button class="btn btn-color"  ng-click="currentbreedingSireClick()">View Sire</button> -->
                  <div class="panel panel-default">
                    <div class="panel-body">
                      <div class="table-responsive">
                        <h4><b>This list will show up to 10 breeding sires. Animals can be added from the other tabs.</b></h4>
                        <table class="table table-bordered table-striped table-hover" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance"  width="100%">
                          <thead>
                            <tr>
                              <th>RegNo</th>
                              <th>Animal</th>
                              <th>Action</th>
                            </tr>
                          </thead>
                          <tbody>
                            <tr ng-repeat="cb in cbs">
                              <td><span ng-bind="cb.RegNo"></span></td>
                              <td><span ng-bind="cb.Animal"></span></td>
                              <td>
                                <i class="fa fa-trash btn btn-danger" ng-click="RemoveSireFromBreedingList(cb,reganimal)"></i>
                              </td>
                            </tr>
                          </tbody>
                        </table>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div ng-show="option ==='ssomeone'">
        <div class="row">
          <div class="col-md-12 row">
            <div class="col-md-6">
              <input type="text" class="form-control" placeholder="Search for a member" ng-model="reganimal.name">
            </div>
            <div class="col-md-6">
              <button type="button" class="btn btn-color" ng-click="addmemberSiere(reganimal); showtable3=false">Search</button>
            </div>
          </div>  
        </div>
        <br>
        <div class="container col-md-12" ng-init="showtable2=false" ng-show="showtable2" style="height:100%;padding: 10px;width: 100%;margin-top: 0;padding: 0;border-radius: 0;">
          <div class="panel panel-default">
            <div class="panel-body">
              <div class="table-responsive">
                <table class="table table-bordered table-striped table-hover" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance"  width="100%">
                  <thead>
                    <tr>
                      <th>Last Name </th>
                      <th>City</th>
                      <th>Ranch</th>
                      <th>State</th>
                      <th>Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr ng-repeat="someone in someonelist">
                      <td><span ng-bind="someone.Lastname"></span></td>
                      <td><span ng-bind="someone.city"></span></td>
                      <td><span ng-bind="someone.RanchName"></span></td>
                      <td><span ng-bind="someone.State"></span></td>
                      <td>
                        <i class="btn btn-success fa fa-plus" ng-click="AddSomeoneSiere(someone)"></i>
                      </td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
        <div ng-init="showtable3=false" ng-show="showtable3">
          <ul class="nav nav-tabs">
            <li class="s" ng-class="{'active': editSearchSire =='e'}" ng-click="editSearchSire = 'e'"><a >Currrent</a></li>
            <li class="s" ng-class="{'active': editSearchSire =='f'}" ng-click="editSearchSire = 'f'"><a >Previous </a></li>
            <li class="s" ng-class="{'active': editSearchSire =='g'}" ng-click="editSearchSire = 'g'"><a >Deceased </a></li>
            <li class="s" ng-class="{'active': editSearchSire =='h'}" ng-click="editSearchSire = 'h'"><a >Current Breeding </a></li>
          </ul>
          <div class="tab-content">
            <div id="currentsireSearch" class="tab-pane fade in" ng-class="{'active': editSearchSire =='e'}">
              <div class="container edge col-md-12" style="height:100%;padding: 10px;width: 100%;margin-top: 0;padding: 0;border-radius: 0">
                <div class="panel panel-default">
                  <div class="panel-body">
                    <div class="table-responsive">
                      <table class="table table-bordered table-striped table-hover" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance"  width="100%">
                        <thead>
                          <tr>
                            <th ng-show="dontshow">MemberNumber</th>
                            <th>RegNo</th>
                            <th>Animal</th>
                            <th>Add To Current Breeding List</th>
                            <th>Action</th>
                          </tr>
                        </thead>
                        <tbody>
                          <tr ng-repeat="scurrent in currentsomeone">
                            <td ng-show="dontshow">{{scurrent.MemberNumber}}</td>
                            <td>{{scurrent.RegNo}}</td>
                            <td>{{scurrent.Animal}}</td>
                            <td>
                              <i class="btn btn-success fa fa-plus" ng-click="SomeoneAddCurrentSireToBreeding(scurrent,reganimal)"></i>
                            </td>
                            <td>
                              <i class="btn btn-success fa fa-plus" ng-click="SomeoneAddCurrentSireToList(scurrent,reganimal)"></i>
                            </td>
                          </tr>
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div id="previoussireSearch" class="tab-pane fade in" ng-class="{'active': editSearchSire =='f'}">
              <div class="container col-md-12" style="height:100%;padding: 10px;width: 100%;margin-top: 0;padding: 0;border-radius: 0">
                <div class="panel panel-default">
                  <div class="panel-body">
                    <div class="table-responsive">
                      <table class="table table-bordered table-striped table-hover" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance"  width="100%">
                        <thead>
                          <tr>
                            <th ng-show="dontshow">MemberNumber</th>
                            <th>RegNo</th>
                            <th>Animal</th>
                            <th>Owner</th>
                            <th>Action</th>
                          </tr>
                        </thead>
                        <tbody>
                          <tr ng-repeat="sprevious in previoussomeone">
                            <td ng-show="dontshow">{{sprevious.MemberNumber}}</td>
                            <td><span ng-bind="sprevious.RegNo"></span></td>
                            <td><span ng-bind="sprevious.Animal"></span></td>
                            <td><span ng-bind="sprevious.MemberName"></span></td>
                            <td>
                              <i class="fa fa-plus btn btn-success" ng-click="SomeoneAddPreviousSireToList(sprevious,reganimal)"></i>
                            </td>
                          </tr>
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div id="deceasedsireSearch" class="tab-pane fade in" ng-class="{'active': editSearchSire =='g'}">
              <div class="container col-md-12" style="height:100%;padding: 10px;width: 100%;margin-top: 0;padding: 0;border-radius: 0">
                <div class="panel panel-default">
                  <div class="panel-body">
                    <div class="table-responsive">
                      <table class="table table-bordered table-striped table-hover" datatable="ng" dt-options="vm.dtOptions" dt-instance="vm.dtInstance"  width="100%">
                        <thead>
                          <tr>
                            <th ng-show="dontshow">MemberNumber</th>
                            <th>RegNo</th>
                            <th>Animal</th>
                            <th>Action</th>
                          </tr>
                        </thead>
                        <tbody>
                          <tr ng-repeat="sdeceased in deceasedsomeone">
                            <td ng-show="dontshow">{{sdeceased.MemberNumber}}</td>
                            <td><span ng-bind="sdeceased.RegNo"></span></td>
                            <td><span ng-bind="sdeceased.Animal"></span></td>
                            <td>
                              <i class="fa fa-plus btn btn-success" ng-click="SomeoneAddDeceasedSireToList(sdeceased,reganimal)"></i>
                            </td>
                          </tr>
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div id="currentbreedingsireSearch" class="tab-pane fade in" ng-class="{'active': editSearchSire =='h'}">
              <div class="container col-md-12" style="height:100%;padding: 10px;width: 100%;margin-top: 0;padding: 0;border-radius: 0">
                <div class="panel panel-default">
                  <div class="panel-body">
                    <div class="table-responsive">
